  rdfs:label "SubLoadAreas";
  cims:multiplicity cims:M:1..n;
  owl:inverseOf cim:SubLoadArea.LoadArea .

cim:LoadBreakSwitch a owl:Class;
  rdfs:comment "A mechanical switching device capable of making, carrying, and breaking currents under normal operating conditions."@en;
  rdfs:subClassOf cim:ProtectedSwitch;
  rdfs:label "LoadBreakSwitch" .

cim:LoadGroup a owl:Class;
  rdfs:comment "The class is the third level in a hierarchical structure for grouping of loads for the purpose of load flow load scaling."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "LoadGroup" .

cim:LoadGroup.SubLoadArea a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The SubLoadArea where the Loadgroup belongs."@en;
  rdfs:domain cim:LoadGroup;
  rdfs:range cim:SubLoadArea;
  rdfs:label "SubLoadArea";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:SubLoadArea.LoadGroups .

cim:LoadResponseCharacteristic a owl:Class;
  rdfs:comment "Models the characteristic response of the load demand due to changes in system conditions such as voltage and frequency. It is not related to demand response. If LoadResponseCharacteristic.exponentModel is True, the exponential voltage or frequency dependent models are specified and used as to calculate active and reactive power components of the load model. The equations to calculate active and reactive power components of the load model are internal to the power flow calculation, hence they use"@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "LoadResponseCharacteristic" .

cim:LoadResponseCharacteristic.EnergyConsumer a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The set of loads that have the response characteristics."@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range cim:EnergyConsumer;
  rdfs:label "EnergyConsumer";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:EnergyConsumer.LoadResponse .

cim:LoadResponseCharacteristic.exponentModel a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Indicates the exponential voltage dependency model is to be used. If false, the coefficient model is to be used. The exponential voltage dependency model consist of the attributes: - pVoltageExponent - qVoltageExponent - pFrequencyExponent - qFrequencyExponent. The coefficient model consist of the attributes: - pConstantImpedance - pConstantCurrent - pConstantPower - qConstantImpedance - qConstantCurrent - qConstantPower. The sum of pConstantImpedance, pConstantCurrent and pConstantPower shall equal"@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range xsd:boolean;
  rdfs:label "exponentModel";
  cims:multiplicity cims:M:1..1 .

cim:LoadResponseCharacteristic.pConstantCurrent a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Portion of active power load modelled as constant current."@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range xsd:float;
  rdfs:label "pConstantCurrent";
  cims:multiplicity cims:M:0..1 .

cim:LoadResponseCharacteristic.pConstantImpedance a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Portion of active power load modelled as constant impedance."@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range xsd:float;
  rdfs:label "pConstantImpedance";
  cims:multiplicity cims:M:0..1 .

cim:LoadResponseCharacteristic.pConstantPower a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Portion of active power load modelled as constant power."@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range xsd:float;
  rdfs:label "pConstantPower";
  cims:multiplicity cims:M:0..1 .

cim:LoadResponseCharacteristic.qConstantCurrent a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Portion of reactive power load modelled as constant current."@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range xsd:float;
  rdfs:label "qConstantCurrent";
  cims:multiplicity cims:M:0..1 .

cim:LoadResponseCharacteristic.qConstantImpedance a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Portion of reactive power load modelled as constant impedance."@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range xsd:float;
  rdfs:label "qConstantImpedance";
  cims:multiplicity cims:M:0..1 .

cim:LoadResponseCharacteristic.qConstantPower a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Portion of reactive power load modelled as constant power."@en;
  rdfs:domain cim:LoadResponseCharacteristic;
  rdfs:range xsd:float;
  rdfs:label "qConstantPower";
  cims:multiplicity cims:M:0..1 .

cim:Location a owl:Class;
  rdfs:comment "The place, scene, or point of something where someone or something has been, is, and/or will be at a given moment in time. It can be defined with one or more position points (coordinates) in a given coordinate system."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "Location" .

cim:Location.CoordinateSystem a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Coordinate system used to describe position points of this location."@en;
  rdfs:domain cim:Location;
  rdfs:range cim:CoordinateSystem;
  rdfs:label "CoordinateSystem";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:CoordinateSystem.Locations .

cim:Location.PowerSystemResources a owl:ObjectProperty, owl:InverseFunctionalProperty,
    owl:FunctionalProperty;
  rdfs:comment "All power system resources at this location."@en;
  rdfs:domain cim:Location;
  rdfs:range cim:PowerSystemResource;
  rdfs:label "PowerSystemResources";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:PowerSystemResource.Location .

cim:Manufacturer a owl:Class;
  rdfs:comment "Organisation that manufactures asset products."@en;
  rdfs:subClassOf cim:OrganisationRole;
  rdfs:label "Manufacturer" .

cim:Manufacturer.ProductAssetModels a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "All asset models by this manufacturer."@en;
  rdfs:domain cim:Manufacturer;
  rdfs:range cim:ProductAssetModel;
  rdfs:label "ProductAssetModels";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:ProductAssetModel.Manufacturer .

cim:Measurement a owl:Class;
  rdfs:comment "A Measurement represents any measured, calculated or non-measured non-calculated quantity. Any piece of equipment may contain Measurements, e.g. a substation may have temperature measurements and door open indications, a transformer may have oil temperature and tank pressure measurements, a bay may contain a number of power flow measurements and a Breaker may contain a switch status measurement.  The PSR - Measurement association is intended to capture this use of Measurement and is included in the"@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "Measurement" .

cim:Measurement.PowerSystemResource a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The power system resource that contains the measurement."@en;
  rdfs:domain cim:Measurement;
  rdfs:range cim:PowerSystemResource;
  rdfs:label "PowerSystemResource";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:PowerSystemResource.Measurements .

cim:Measurement.measurementType a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Specifies the type of measurement.  For example, this specifies if the measurement represents an indoor temperature, outdoor temperature, bus voltage, line flow, etc.
When the measurementType is set to \"Specialization\", the type of Measurement is defined in more detail by the specialized class which inherits from Measurement."""@en;
  rdfs:domain cim:Measurement;
  rdfs:range xsd:string;
  rdfs:label "measurementType";
  cims:multiplicity cims:M:1..1;
  skos:example "CurrencyExchange-Actual", "Price-Actual", "ThreePhaseActivePower", "ThreePhaseActivePower-Flow-Estimated";
  cims:pragmatics "Possible values: CurrencyExchange-Actual, Price-Actual, ThreePhaseActivePower, ThreePhaseActivePower-Flow-Estimated" .

cim:Measurement.phases a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment """Indicates to which phases the measurement applies and avoids the need to use 'measurementType' to also encode phase information (which would explode the types). The phase information in Measurement, along with 'measurementType' and 'phases' uniquely defines a Measurement for a device, based on normal network phase. Their meaning will not change when the computed energizing phasing is changed due to jumpers or other reasons.
If the attribute is missing three phases (ABC) shall be assumed."""@en;
  rdfs:domain cim:Measurement;
  rdfs:range cim:PhaseCode;
  rdfs:label "phases";
  cims:multiplicity cims:M:0..1 .

cim:Measurement.unitMultiplier a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The unit multiplier of the measured quantity."@en;
  rdfs:domain cim:Measurement;
  rdfs:range cim:UnitMultiplier;
  rdfs:label "unitMultiplier";
  cims:multiplicity cims:M:1..1 .

cim:Measurement.unitSymbol a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The unit of measure of the measured quantity."@en;
  rdfs:domain cim:Measurement;
  rdfs:range cim:UnitSymbol;
  rdfs:label "unitSymbol";
  cims:multiplicity cims:M:1..1 .

cim:MeasurementValue a owl:Class;
  rdfs:comment "The current state for a measurement. A state value is an instance of a measurement from a specific source. Measurements can be associated with many state values, each representing a different source for the measurement."@en;
  rdfs:subClassOf cim:IOPoint;
  rdfs:label "MeasurementValue" .

cim:MeasurementValue.MeasurementValueSource a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "A reference to the type of source that updates the MeasurementValue, e.g. SCADA, CCLink, manual, etc. User conventions for the names of sources are contained in the introduction to IEC 61970-301."@en;
  rdfs:domain cim:MeasurementValue;
  rdfs:range cim:MeasurementValueSource;
  rdfs:label "MeasurementValueSource";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:MeasurementValueSource.MeasurementValues .

cim:MeasurementValue.sensorAccuracy a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The limit, expressed as a percentage of the sensor maximum, that errors will not exceed when the sensor is used under  reference conditions."@en;
  rdfs:domain cim:MeasurementValue;
  rdfs:range xsd:float;
  rdfs:label "sensorAccuracy";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.none;
  qudt:hasQuantityKind cim:PerCent;
  qudt:hasUnit unit:PERCENT .

cim:MeasurementValue.timeStamp a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The time when the value was last updated."@en;
  rdfs:domain cim:MeasurementValue;
  rdfs:range xsd:dateTime;
  rdfs:label "timeStamp";
  cims:multiplicity cims:M:0..1 .

cim:MeasurementValueSource a owl:Class;
  rdfs:comment "MeasurementValueSource describes the alternative sources updating a MeasurementValue. User conventions for how to use the MeasurementValueSource attributes are defined in IEC 61970-301."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "MeasurementValueSource" .

cim:MeasurementValueSource.MeasurementValues a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The MeasurementValues updated by the source."@en;
  rdfs:domain cim:MeasurementValueSource;
  rdfs:range cim:MeasurementValue;
  rdfs:label "MeasurementValues";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:MeasurementValue.MeasurementValueSource .

cim:Meter a owl:Class;
  rdfs:comment "Physical asset that performs the metering role of the usage point. Used for measuring consumption and detection of events."@en;
  rdfs:subClassOf cim:EndDevice;
  rdfs:label "Meter" .

cim:Name a owl:Class;
  rdfs:comment "The Name class provides the means to define any number of human readable  names for an object. A name is <b>not</b> to be used for defining inter-object relationships. For inter-object relationships instead use the object identification 'mRID'."@en;
  rdfs:label "Name" .

cim:Name.name a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Any free text that used as a name or alternative identifier of the object."@en;
  rdfs:domain cim:Name;
  rdfs:range xsd:string;
  rdfs:label "name";
  cims:multiplicity cims:M:1..1 .

cim:NameType a owl:Class;
  rdfs:comment "Type of name. Possible values for attribute 'name' are implementation dependent but standard profiles may specify types. An enterprise may have multiple IT systems each having its own local name for the same object, e.g. a planning system may have different names from an EMS. An object may also have different names within the same IT system, e.g. localName as defined in CIM version 14. The definition from CIM14 is: The localName is a human readable name of the object. It is a free text name local"@en;
  rdfs:label "NameType" .

cim:NameType.description a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Description of the name type."@en;
  rdfs:domain cim:NameType;
  rdfs:range xsd:string;
  rdfs:label "description";
  cims:multiplicity cims:M:0..1 .

cim:NameType.name a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Name of the name type."@en;
  rdfs:domain cim:NameType;
  rdfs:range xsd:string;
  rdfs:label "name";
  cims:multiplicity cims:M:1..1 .

cim:NonConformLoad a owl:Class;
  rdfs:comment "NonConformLoad represents loads that do not follow a daily load change pattern and whose changes are not correlated with the daily load change pattern."@en;
  rdfs:subClassOf cim:EnergyConsumer;
  rdfs:label "NonConformLoad" .

cim:NonConformLoad.LoadGroup a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Group of this ConformLoad."@en;
  rdfs:domain cim:NonConformLoad;
  rdfs:range cim:NonConformLoadGroup;
  rdfs:label "LoadGroup";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:NonConformLoadGroup.EnergyConsumers .

cim:NonConformLoadGroup a owl:Class;
  rdfs:comment "Loads that do not follow a daily and seasonal load variation pattern."@en;
  rdfs:subClassOf cim:LoadGroup;
  rdfs:label "NonConformLoadGroup" .

cim:NonConformLoadGroup.EnergyConsumers a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Conform loads assigned to this ConformLoadGroup."@en;
  rdfs:domain cim:NonConformLoadGroup;
  rdfs:range cim:NonConformLoad;
  rdfs:label "EnergyConsumers";
  cims:multiplicity cims:M:1..n;
  owl:inverseOf cim:NonConformLoad.LoadGroup .

cim:OperationalLimit a owl:Class;
  rdfs:comment "A value and normal value associated with a specific kind of limit.  The sub class value and normalValue attributes vary inversely to the associated OperationalLimitType.acceptableDuration (acceptableDuration for short).   If a particular piece of equipment has multiple operational limits of the same kind (apparent power, current, etc.), the limit with the greatest acceptableDuration shall have the smallest limit value and the limit with the smallest acceptableDuration shall have the largest limit"@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "OperationalLimit" .

cim:OperationalLimit.OperationalLimitSet a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The limit set to which the limit values belong."@en;
  rdfs:domain cim:OperationalLimit;
  rdfs:range cim:OperationalLimitSet;
  rdfs:label "OperationalLimitSet";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:OperationalLimitSet.OperationalLimitValue .

cim:OperationalLimit.OperationalLimitType a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The limit type associated with this limit."@en;
  rdfs:domain cim:OperationalLimit;
  rdfs:range cim:OperationalLimitType;
  rdfs:label "OperationalLimitType";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:OperationalLimitType.OperationalLimit .

cim:OperationalLimitDirectionKind a owl:Class;
  rdfs:comment "The direction attribute describes the side of  a limit that is a violation."@en;
  rdfs:label "OperationalLimitDirectionKind" .

cim:OperationalLimitDirectionKind.absoluteValue a cim:OperationalLimitDirectionKind;
  rdfs:comment "An absoluteValue limit means that a monitored absolute value above the limit value is a violation."@en;
  rdfs:label "absoluteValue" .

cim:OperationalLimitDirectionKind.high a cim:OperationalLimitDirectionKind;
  rdfs:comment "High means that a monitored value above the limit value is a violation.   If applied to a terminal flow, the positive direction is into the terminal."@en;
  rdfs:label "high" .

cim:OperationalLimitDirectionKind.low a cim:OperationalLimitDirectionKind;
  rdfs:comment "Low means a monitored value below the limit is a violation.  If applied to a terminal flow, the positive direction is into the terminal."@en;
  rdfs:label "low" .

cim:OperationalLimitSet a owl:Class;
  rdfs:comment "A set of limits associated with equipment.  Sets of limits might apply to a specific temperature, or season for example. A set of limits may contain different severities of limit levels that would apply to the same equipment. The set may contain limits of different types such as apparent power and current limits or high and low voltage limits  that are logically applied together as a set."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "OperationalLimitSet" .

cim:OperationalLimitSet.OperationalLimitValue a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Values of equipment limits."@en;
  rdfs:domain cim:OperationalLimitSet;
  rdfs:range cim:OperationalLimit;
  rdfs:label "OperationalLimitValue";
  cims:multiplicity cims:M:0..n, cims:M:1..n;
  owl:inverseOf cim:OperationalLimit.OperationalLimitSet .

cim:OperationalLimitSet.Terminal a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The terminal where the operational limit set apply."@en;
  rdfs:domain cim:OperationalLimitSet;
  rdfs:range cim:ACDCTerminal;
  rdfs:label "Terminal";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:ACDCTerminal.OperationalLimitSet .

cim:OperationalLimitType a owl:Class;
  rdfs:comment "The operational meaning of a category of limits."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "OperationalLimitType" .

cim:OperationalLimitType.OperationalLimit a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The operational limits associated with this type of limit."@en;
  rdfs:domain cim:OperationalLimitType;
  rdfs:range cim:OperationalLimit;
  rdfs:label "OperationalLimit";
  cims:multiplicity cims:M:0..n, cims:M:1..n;
  owl:inverseOf cim:OperationalLimit.OperationalLimitType .

cim:OperationalLimitType.acceptableDuration a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The nominal acceptable duration of the limit. Limits are commonly expressed in terms of the time limit for which the limit is normally acceptable. The actual acceptable duration of a specific limit may depend on other local factors such as temperature or wind speed. The attribute has meaning only if the flag isInfiniteDuration is set to false, hence it shall not be exchanged when isInfiniteDuration is set to true."@en;
  rdfs:domain cim:OperationalLimitType;
  rdfs:range xsd:float;
  rdfs:label "acceptableDuration";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.s;
  qudt:hasQuantityKind cim:Seconds;
  qudt:hasUnit unit:SEC .

cim:OperationalLimitType.direction a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The direction of the limit."@en;
  rdfs:domain cim:OperationalLimitType;
  rdfs:range cim:OperationalLimitDirectionKind;
  rdfs:label "direction";
  cims:multiplicity cims:M:1..1 .

cim:OperationalLimitType.isInfiniteDuration a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Defines if the operational limit type has infinite duration. If true, the limit has infinite duration. If false, the limit has definite duration which is defined by the attribute acceptableDuration."@en;
  rdfs:domain cim:OperationalLimitType;
  rdfs:range xsd:boolean;
  rdfs:label "isInfiniteDuration";
  cims:multiplicity cims:M:1..1 .

cim:Organisation a owl:Class;
  rdfs:comment "Organisation that might have roles as utility, contractor, supplier, manufacturer, customer, etc."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "Organisation" .

cim:Organisation.Roles a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "All roles of this organisation."@en;
  rdfs:domain cim:Organisation;
  rdfs:range cim:OrganisationRole;
  rdfs:label "Roles";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:OrganisationRole.Organisation .

cim:OrganisationRole a owl:Class;
  rdfs:comment "Identifies a way in which an organisation may participate in the utility enterprise (e.g., customer, manufacturer, etc)."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "OrganisationRole" .

cim:OrganisationRole.Organisation a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Organisation having this role."@en;
  rdfs:domain cim:OrganisationRole;
  rdfs:range cim:Organisation;
  rdfs:label "Organisation";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:Organisation.Roles .

cim:OrientationKind a owl:Class;
  rdfs:comment "The orientation of the coordinate system with respect to top, left, and the coordinate number system."@en;
  rdfs:label "OrientationKind" .

cim:OrientationKind.negative a cim:OrientationKind;
  rdfs:comment "For 2D diagrams, a negative orientation gives the left-hand orientation (favoured by computer graphics displays) with X values increasing from left to right and Y values increasing from top to bottom.   This is also known as a left hand orientation."@en;
  rdfs:label "negative" .

cim:OverheadWireInfo a owl:Class;
  rdfs:comment "Overhead wire data."@en;
  rdfs:subClassOf cim:WireInfo;
  rdfs:label "OverheadWireInfo" .

cim:PetersenCoil a owl:Class;
  rdfs:comment "A variable impedance device normally used to offset line charging during single line faults in an ungrounded section of network."@en;
  rdfs:subClassOf cim:EarthFaultCompensator;
  rdfs:label "PetersenCoil" .

cim:PetersenCoilModeKind a owl:Class;
  rdfs:comment "The mode of operation for a Petersen coil."@en;
  rdfs:label "PetersenCoilModeKind" .

cim:PhaseCode a owl:Class;
  rdfs:comment "An unordered enumeration of phase identifiers.  Allows designation of phases for both transmission and distribution equipment, circuits and loads.   The enumeration, by itself, does not describe how the phases are connected together or connected to ground.  Ground is not explicitly denoted as a phase. Residential and small commercial loads are often served from single-phase, or split-phase, secondary circuits. For the example of s12N, phases 1 and 2 refer to hot wires that are 180 degrees out of"@en;
  rdfs:label "PhaseCode" .

cim:PhaseCode.A a cim:PhaseCode;
  rdfs:comment "Phase A."@en;
  rdfs:label "A" .

cim:PhaseCode.AB a cim:PhaseCode;
  rdfs:comment "Phases A and B."@en;
  rdfs:label "AB" .

cim:PhaseCode.ABC a cim:PhaseCode;
  rdfs:comment "Phases A, B, and C."@en;
  rdfs:label "ABC" .

cim:PhaseCode.ABCN a cim:PhaseCode;
  rdfs:comment "Phases A, B, C, and N."@en;
  rdfs:label "ABCN" .

cim:PhaseCode.AC a cim:PhaseCode;
  rdfs:comment "Phases A and C."@en;
  rdfs:label "AC" .

cim:PhaseCode.B a cim:PhaseCode;
  rdfs:comment "Phase B."@en;
  rdfs:label "B" .

cim:PhaseCode.BC a cim:PhaseCode;
  rdfs:comment "Phases B and C."@en;
  rdfs:label "BC" .

cim:PhaseCode.C a cim:PhaseCode;
  rdfs:comment "Phase C."@en;
  rdfs:label "C" .

cim:PhaseCode.N a cim:PhaseCode;
  rdfs:comment "Neutral phase."@en;
  rdfs:label "N" .

cim:PhaseCode.none a cim:PhaseCode;
  rdfs:comment "No phases specified."@en;
  rdfs:label "none" .

cim:PhaseConnectedFaultKind a owl:Class;
  rdfs:comment "The type of fault connection among phases."@en;
  rdfs:label "PhaseConnectedFaultKind" .

cim:PotentialTransformer a owl:Class;
  rdfs:comment "Instrument transformer (also known as Voltage Transformer) used to measure electrical qualities of the circuit that is being protected and/or monitored. Typically used as voltage transducer for the purpose of metering, protection, or sometimes auxiliary substation supply. A typical secondary voltage rating would be 120V."@en;
  rdfs:subClassOf cim:Sensor;
  rdfs:label "PotentialTransformer" .

cim:PowerSystemResource a owl:Class;
  rdfs:comment "A power system resource (PSR) can be an item of equipment such as a switch, an equipment container containing many individual items of equipment such as a substation, or an organisational entity such as sub-control area. Power system resources can have measurements associated."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "PowerSystemResource" .

cim:PowerSystemResource.Assets a owl:ObjectProperty;
  rdfs:comment "All assets represented by this power system resource. For example, multiple conductor assets are electrically modelled as a single AC line segment."@en;
  rdfs:domain cim:PowerSystemResource;
  rdfs:range cim:Asset;
  rdfs:label "Assets";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Asset.PowerSystemResources .

cim:PowerSystemResource.Location a owl:ObjectProperty, owl:InverseFunctionalProperty,
    owl:FunctionalProperty;
  rdfs:comment "Location of this power system resource."@en;
  rdfs:domain cim:PowerSystemResource;
  rdfs:range cim:Location;
  rdfs:label "Location";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:Location.PowerSystemResources .

cim:PowerSystemResource.Measurements a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The measurements associated with this power system resource."@en;
  rdfs:domain cim:PowerSystemResource;
  rdfs:range cim:Measurement;
  rdfs:label "Measurements";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Measurement.PowerSystemResource .

cim:PowerTransformer a owl:Class;
  rdfs:comment "An electrical device consisting of  two or more coupled windings, with or without a magnetic core, for introducing mutual coupling between electric circuits. Transformers can be used to control voltage and phase shift (active power flow). A power transformer may be composed of separate transformer tanks that need not be identical. A power transformer can be modelled with or without tanks and is intended for use in both balanced and unbalanced representations.   A power transformer typically has two"@en;
  rdfs:subClassOf cim:ConductingEquipment;
  rdfs:label "PowerTransformer" .

cim:PowerTransformer.PowerTransformerEnd a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The ends of this power transformer."@en;
  rdfs:domain cim:PowerTransformer;
  rdfs:range cim:PowerTransformerEnd;
  rdfs:label "PowerTransformerEnd";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:PowerTransformerEnd.PowerTransformer .

cim:PowerTransformer.isPartOfGeneratorUnit a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Indicates whether the machine is part of a power station unit. Used for short circuit data exchange according to IEC 60909.  It has an impact on how the correction factors are calculated for transformers, since the transformer is not necessarily part of a synchronous machine and generating unit. It is not always possible to derive this information from the model. This is why the attribute is necessary."@en;
  rdfs:domain cim:PowerTransformer;
  rdfs:range xsd:boolean;
  rdfs:label "isPartOfGeneratorUnit";
  cims:multiplicity cims:M:1..1 .

cim:PowerTransformerEnd a owl:Class;
  rdfs:comment "A PowerTransformerEnd is associated with each Terminal of a PowerTransformer. The impedance values r, r0, x, and x0 of a PowerTransformerEnd represents a star equivalent as follows. 1) for a two Terminal PowerTransformer the high voltage (TransformerEnd.endNumber=1) PowerTransformerEnd has non zero values on r, r0, x, and x0 while the low voltage (TransformerEnd.endNumber=2) PowerTransformerEnd has zero values for r, r0, x, and x0.  Parameters are always provided, even if the PowerTransformerEnds"@en;
  rdfs:subClassOf cim:TransformerEnd;
  rdfs:label "PowerTransformerEnd" .

cim:PowerTransformerEnd.PowerTransformer a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The power transformer of this power transformer end."@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range cim:PowerTransformer;
  rdfs:label "PowerTransformer";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:PowerTransformer.PowerTransformerEnd .

cim:PowerTransformerEnd.b a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Magnetizing branch susceptance (B mag).  The value can be positive or negative."@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range xsd:float;
  rdfs:label "b";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.S;
  qudt:hasQuantityKind cim:Susceptance;
  qudt:hasUnit unit:S .

cim:PowerTransformerEnd.connectionKind a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Kind of connection."@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range cim:WindingConnection;
  rdfs:label "connectionKind";
  cims:multiplicity cims:M:0..1 .

cim:PowerTransformerEnd.g a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Magnetizing branch conductance."@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range xsd:float;
  rdfs:label "g";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.S;
  qudt:hasQuantityKind cim:Conductance;
  qudt:hasUnit unit:S .

cim:PowerTransformerEnd.r a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Resistance (star-model) of the transformer end.
The attribute shall be equal to or greater than zero for non-equivalent transformers."""@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range xsd:float;
  rdfs:label "r";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Resistance;
  qudt:hasUnit unit:OHM .

cim:PowerTransformerEnd.ratedS a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Normal apparent power rating.
The attribute shall be a positive value. For a two-winding transformer the values for the high and low voltage sides shall be identical."""@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range xsd:float;
  rdfs:label "ratedS";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VA;
  qudt:hasQuantityKind cim:ApparentPower;
  qudt:hasUnit unit:MegaV-A .

cim:PowerTransformerEnd.ratedU a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Rated voltage: phase-phase for three-phase windings, and either phase-phase or phase-neutral for single-phase windings.
A high voltage side, as given by TransformerEnd.endNumber, shall have a ratedU that is greater than or equal to ratedU for the lower voltage sides.
The attribute shall be a positive value."""@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range xsd:float;
  rdfs:label "ratedU";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.k;
  cim:unitSymbol cim:UnitSymbol.V;
  qudt:hasQuantityKind cim:Voltage;
  qudt:hasUnit unit:KiloV .

cim:PowerTransformerEnd.x a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Positive sequence series reactance (star-model) of the transformer end."@en;
  rdfs:domain cim:PowerTransformerEnd;
  rdfs:range xsd:float;
  rdfs:label "x";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Reactance;
  qudt:hasUnit unit:OHM .

cim:ProductAssetModel a owl:Class;
  rdfs:comment "Asset model by a specific manufacturer."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "ProductAssetModel" .

cim:ProductAssetModel.AssetInfo a owl:ObjectProperty, owl:InverseFunctionalProperty,
    owl:FunctionalProperty;
  rdfs:comment "Asset information (nameplate) for this product asset model."@en;
  rdfs:domain cim:ProductAssetModel;
  rdfs:range cim:AssetInfo;
  rdfs:label "AssetInfo";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:AssetInfo.ProductAssetModel .

cim:ProductAssetModel.Manufacturer a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Manufacturer of this asset model."@en;
  rdfs:domain cim:ProductAssetModel;
  rdfs:range cim:Manufacturer;
  rdfs:label "Manufacturer";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:Manufacturer.ProductAssetModels .

cim:ProductAssetModel.modelNumber a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Manufacturer's model number."@en;
  rdfs:domain cim:ProductAssetModel;
  rdfs:label "modelNumber";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:String .

cim:ProductAssetModel.modelVersion a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Version number for product model, which indicates vintage of the product."@en;
  rdfs:domain cim:ProductAssetModel;
  rdfs:label "modelVersion";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:String .

cim:ProductAssetModel.usageKind a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Intended usage for this asset model."@en;
  rdfs:domain cim:ProductAssetModel;
  rdfs:range cim:AssetModelUsageKind;
  rdfs:label "usageKind";
  cims:multiplicity cims:M:0..1 .

cim:ProtectedSwitch a owl:Class;
  rdfs:comment "A ProtectedSwitch is a switching device that can be operated by ProtectionEquipment."@en;
  rdfs:subClassOf cim:Switch;
  rdfs:label "ProtectedSwitch" .

cim:RatioTapChanger a owl:Class;
  rdfs:comment """A tap changer that changes the voltage ratio impacting the voltage magnitude but not the phase angle across the transformer.

Angle sign convention (general): Positive value indicates a positive phase shift from the winding where the tap is located to the other winding (for a two-winding transformer)."""@en;
  rdfs:subClassOf cim:TapChanger;
  rdfs:label "RatioTapChanger" .

cim:RatioTapChanger.TransformerEnd a owl:ObjectProperty, owl:InverseFunctionalProperty,
    owl:FunctionalProperty;
  rdfs:comment "Transformer end to which this ratio tap changer belongs."@en;
  rdfs:domain cim:RatioTapChanger;
  rdfs:range cim:TransformerEnd;
  rdfs:label "TransformerEnd";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:TransformerEnd.RatioTapChanger .

cim:RatioTapChanger.stepVoltageIncrement a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Tap step increment, in per cent of rated voltage of the power transformer end, per step position.
When the increment is negative, the voltage decreases when the tap step increases."""@en;
  rdfs:domain cim:RatioTapChanger;
  rdfs:range xsd:float;
  rdfs:label "stepVoltageIncrement";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.none;
  qudt:hasQuantityKind cim:PerCent;
  qudt:hasUnit unit:PERCENT .

cim:RegulatingCondEq a owl:Class;
  rdfs:comment "A type of conducting equipment that can regulate a quantity (i.e. voltage or flow) at a specific point in the network."@en;
  rdfs:subClassOf cim:EnergyConnection;
  rdfs:label "RegulatingCondEq" .

cim:RegulatingCondEq.RegulatingControl a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The regulating control scheme in which this equipment participates."@en;
  rdfs:domain cim:RegulatingCondEq;
  rdfs:range cim:RegulatingControl;
  rdfs:label "RegulatingControl";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:RegulatingControl.RegulatingCondEq .

cim:RegulatingCondEq.controlEnabled a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Specifies the regulation status of the equipment.  True is regulating, false is not regulating."@en;
  rdfs:domain cim:RegulatingCondEq;
  rdfs:range xsd:boolean;
  rdfs:label "controlEnabled";
  cims:multiplicity cims:M:1..1 .

cim:RegulatingControl a owl:Class;
  rdfs:comment "Specifies a set of equipment that works together to control a power system quantity such as voltage or flow.  Remote bus voltage control is possible by specifying the controlled terminal located at some place remote from the controlling equipment. The specified terminal shall be associated with the connectivity node of the controlled point.  The most specific subtype of RegulatingControl shall be used in case such equipment participate in the control, e.g. TapChangerControl for tap changers. For"@en;
  rdfs:subClassOf cim:PowerSystemResource;
  rdfs:label "RegulatingControl" .

cim:RegulatingControl.RegulatingCondEq a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The equipment that participates in this regulating control scheme."@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range cim:RegulatingCondEq;
  rdfs:label "RegulatingCondEq";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:RegulatingCondEq.RegulatingControl .

cim:RegulatingControl.Terminal a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The terminal associated with this regulating control.  The terminal is associated instead of a node, since the terminal could connect into either a topological node or a connectivity node.  Sometimes it is useful to model regulation at a terminal of a bus bar object."@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range cim:Terminal;
  rdfs:label "Terminal";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Terminal.RegulatingControl .

cim:RegulatingControl.discrete a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The regulation is performed in a discrete mode. This applies to equipment with discrete controls, e.g. tap changers and shunt compensators."@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range xsd:boolean;
  rdfs:label "discrete";
  cims:multiplicity cims:M:1..1 .

cim:RegulatingControl.enabled a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The flag tells if regulation is enabled."@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range xsd:boolean;
  rdfs:label "enabled";
  cims:multiplicity cims:M:1..1 .

cim:RegulatingControl.mode a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The regulating control mode presently available.  This specification allows for determining the kind of regulation without need for obtaining the units from a schedule."@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range cim:RegulatingControlModeKind;
  rdfs:label "mode";
  cims:multiplicity cims:M:1..1 .

cim:RegulatingControl.targetDeadband a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """This is a deadband used with discrete control to avoid excessive update of controls like tap changers and shunt compensator banks while regulating.  The units of those appropriate for the mode. The attribute shall be a positive value or zero. If RegulatingControl.discrete is set to \"false\", the RegulatingControl.targetDeadband is to be ignored.
Note that for instance, if the targetValue is 100 kV and the targetDeadband is 2 kV the range is from 99 to 101 kV."""@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range xsd:float;
  rdfs:label "targetDeadband";
  cims:multiplicity cims:M:0..1 .

cim:RegulatingControl.targetValue a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The target value specified for case input.   This value can be used for the target value without the use of schedules. The value has the units appropriate to the mode attribute."@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range xsd:float;
  rdfs:label "targetValue";
  cims:multiplicity cims:M:1..1 .

cim:RegulatingControl.targetValueUnitMultiplier a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Specify the multiplier for used for the targetValue."@en;
  rdfs:domain cim:RegulatingControl;
  rdfs:range cim:UnitMultiplier;
  rdfs:label "targetValueUnitMultiplier";
  cims:multiplicity cims:M:1..1 .

cim:RegulatingControlModeKind a owl:Class;
  rdfs:comment "The kind of regulation model.   For example regulating voltage, reactive power, active power, etc."@en;
  rdfs:label "RegulatingControlModeKind" .

cim:RegulatingControlModeKind.voltage a cim:RegulatingControlModeKind;
  rdfs:comment "Voltage is specified."@en;
  rdfs:label "voltage" .

cim:RemoteSignalKind a owl:Class;
  rdfs:comment "Type of input signal coming from remote bus."@en;
  rdfs:label "RemoteSignalKind" .

cim:RotatingMachine a owl:Class;
  rdfs:comment "A rotating machine which may be used as a generator or motor."@en;
  rdfs:subClassOf cim:RegulatingCondEq;
  rdfs:label "RotatingMachine" .

cim:RotatingMachine.GeneratingUnit a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "A synchronous machine may operate as a generator and as such becomes a member of a generating unit."@en;
  rdfs:domain cim:RotatingMachine;
  rdfs:range cim:GeneratingUnit;
  rdfs:label "GeneratingUnit";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:GeneratingUnit.RotatingMachine .

cim:RotatingMachine.p a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Active power injection. Load sign convention is used, i.e. positive sign means flow out from a node.
Starting value for a steady state solution."""@en;
  rdfs:domain cim:RotatingMachine;
  rdfs:range xsd:float;
  rdfs:label "p";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:RotatingMachine.q a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Reactive power injection. Load sign convention is used, i.e. positive sign means flow out from a node.
Starting value for a steady state solution."""@en;
  rdfs:domain cim:RotatingMachine;
  rdfs:range xsd:float;
  rdfs:label "q";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:RotatingMachine.ratedS a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Nameplate apparent power rating for the unit.
The attribute shall have a positive value."""@en;
  rdfs:domain cim:RotatingMachine;
  rdfs:range xsd:float;
  rdfs:label "ratedS";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VA;
  qudt:hasQuantityKind cim:ApparentPower;
  qudt:hasUnit unit:MegaV-A .

cim:RotorKind a owl:Class;
  rdfs:comment "Type of rotor on physical machine."@en;
  rdfs:label "RotorKind" .

cim:SVCControlMode a owl:Class;
  rdfs:comment "Static VAr Compensator control mode."@en;
  rdfs:label "SVCControlMode";
  owl:deprecated true .

cim:Sensor a owl:Class;
  rdfs:comment "This class describe devices that transform a measured quantity into signals that can be presented at displays, used in control or be recorded."@en;
  rdfs:subClassOf cim:AuxiliaryEquipment;
  rdfs:label "Sensor" .

cim:ShortCircuitRotorKind a owl:Class;
  rdfs:comment "Type of rotor, used by short circuit applications."@en;
  rdfs:label "ShortCircuitRotorKind" .

cim:ShuntCompensator a owl:Class;
  rdfs:comment "A shunt capacitor or reactor or switchable bank of shunt capacitors or reactors. A section of a shunt compensator is an individual capacitor or reactor. A negative value for bPerSection indicates that the compensator is a reactor. ShuntCompensator is a single terminal device.  Ground is implied."@en;
  rdfs:subClassOf cim:RegulatingCondEq;
  rdfs:label "ShuntCompensator" .

cim:ShuntCompensator.SvShuntCompensatorSections a owl:ObjectProperty, owl:InverseFunctionalProperty,
    owl:FunctionalProperty;
  rdfs:comment "The state for the number of shunt compensator sections in service."@en;
  rdfs:domain cim:ShuntCompensator;
  rdfs:range cim:SvShuntCompensatorSections;
  rdfs:label "SvShuntCompensatorSections";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:SvShuntCompensatorSections.ShuntCompensator .

cim:ShuntCompensator.aVRDelay a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "An automatic voltage regulation delay (AVRDelay) which is the time delay from a change in voltage to when the capacitor is allowed to change state. This filters out temporary changes in voltage."@en;
  rdfs:domain cim:ShuntCompensator;
  rdfs:range xsd:float;
  rdfs:label "aVRDelay";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.s;
  qudt:hasQuantityKind cim:Seconds;
  qudt:hasUnit unit:SEC .

cim:ShuntCompensator.maximumSections a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The maximum number of sections that may be switched in."@en;
  rdfs:domain cim:ShuntCompensator;
  rdfs:range xsd:integer;
  rdfs:label "maximumSections";
  cims:multiplicity cims:M:1..1 .

cim:ShuntCompensator.nomU a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The voltage at which the nominal reactive power may be calculated. This should normally be within 10% of the voltage at which the capacitor is connected to the network."@en;
  rdfs:domain cim:ShuntCompensator;
  rdfs:range xsd:float;
  rdfs:label "nomU";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.k;
  cim:unitSymbol cim:UnitSymbol.V;
  qudt:hasQuantityKind cim:Voltage;
  qudt:hasUnit unit:KiloV .

cim:ShuntCompensator.normalSections a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The normal number of sections switched in. The value shall be between zero and ShuntCompensator.maximumSections."@en;
  rdfs:domain cim:ShuntCompensator;
  rdfs:range xsd:integer;
  rdfs:label "normalSections";
  cims:multiplicity cims:M:1..1 .

cim:ShuntCompensator.sections a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Shunt compensator sections in use. Starting value for steady state solution. The attribute shall be a positive value or zero. Non integer values are allowed to support continuous variables. The reasons for continuous value are to support study cases where no discrete shunt compensators has yet been designed, a solutions where a narrow voltage band force the sections to oscillate or accommodate for a continuous solution as input.  For LinearShuntConpensator the value shall be between zero and ShuntCompensator.maximumSections."@en;
  rdfs:domain cim:ShuntCompensator;
  rdfs:range xsd:float;
  rdfs:label "sections";
  cims:multiplicity cims:M:1..1 .

cim:ShuntCompensatorInfo a owl:Class;
  rdfs:comment "Properties of shunt capacitor, shunt reactor or switchable bank of shunt capacitor or reactor assets."@en;
  rdfs:subClassOf cim:AssetInfo;
  rdfs:label "ShuntCompensatorInfo" .

cim:ShuntCompensatorInfo.ratedReactivePower a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Rated reactive power."@en;
  rdfs:domain cim:ShuntCompensatorInfo;
  rdfs:label "ratedReactivePower";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:ReactivePower .

cim:ShuntCompensatorInfo.ratedVoltage a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Rated voltage."@en;
  rdfs:domain cim:ShuntCompensatorInfo;
  rdfs:label "ratedVoltage";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:Voltage .

cim:SinglePhaseKind a owl:Class;
  rdfs:comment "Enumeration of single phase identifiers. Allows designation of single phases for both transmission and distribution equipment, circuits and loads."@en;
  rdfs:label "SinglePhaseKind" .

cim:Source a owl:Class;
  rdfs:comment "Source gives information related to the origin of a value."@en;
  rdfs:label "Source" .

cim:StaticLoadModelKind a owl:Class;
  rdfs:comment "Type of static load model."@en;
  rdfs:label "StaticLoadModelKind" .

cim:StationSupply a owl:Class;
  rdfs:comment "Station supply with load derived from the station output."@en;
  rdfs:subClassOf cim:EnergyConsumer;
  rdfs:label "StationSupply" .

cim:SubGeographicalRegion a owl:Class;
  rdfs:comment "A subset of a geographical region of a power system network model."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "SubGeographicalRegion" .

cim:SubGeographicalRegion.Lines a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The lines within the sub-geographical region."@en;
  rdfs:domain cim:SubGeographicalRegion;
  rdfs:range cim:Line;
  rdfs:label "Lines";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Line.Region .

cim:SubGeographicalRegion.Region a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The geographical region which this sub-geographical region is within."@en;
  rdfs:domain cim:SubGeographicalRegion;
  rdfs:range cim:GeographicalRegion;
  rdfs:label "Region";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:GeographicalRegion.Regions .

cim:SubGeographicalRegion.Substations a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The substations in this sub-geographical region."@en;
  rdfs:domain cim:SubGeographicalRegion;
  rdfs:range cim:Substation;
  rdfs:label "Substations";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Substation.Region .

cim:SubLoadArea a owl:Class;
  rdfs:comment "The class is the second level in a hierarchical structure for grouping of loads for the purpose of load flow load scaling."@en;
  rdfs:subClassOf cim:EnergyArea;
  rdfs:label "SubLoadArea" .

cim:SubLoadArea.LoadArea a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The LoadArea where the SubLoadArea belongs."@en;
  rdfs:domain cim:SubLoadArea;
  rdfs:range cim:LoadArea;
  rdfs:label "LoadArea";
  cims:multiplicity cims:M:1..1;
