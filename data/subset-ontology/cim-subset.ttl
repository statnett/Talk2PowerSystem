  rdfs:label "ExcST7BUELselectorKind" .

cim:Feeder a owl:Class;
  rdfs:comment """A collection of equipment for organizational purposes, used for grouping distribution resources.
The organization a feeder does not necessarily reflect connectivity or current operation state."""@en;
  rdfs:subClassOf cim:EquipmentContainer;
  rdfs:label "Feeder" .

cim:FrancisGovernorControlKind a owl:Class;
  rdfs:comment "Governor control flag for Francis hydro model."@en;
  rdfs:label "FrancisGovernorControlKind" .

cim:FuelType a owl:Class;
  rdfs:comment "Type of fuel."@en;
  rdfs:label "FuelType" .

cim:Fuse a owl:Class;
  rdfs:comment "An overcurrent protective device with a circuit opening fusible part that is heated and severed by the passage of overcurrent through it. A fuse is considered a switching device because it breaks current."@en;
  rdfs:subClassOf cim:Switch;
  rdfs:label "Fuse" .

cim:GeneratingUnit a owl:Class;
  rdfs:comment "A single or set of synchronous machines for converting mechanical power into alternating-current power. For example, individual machines within a set may be defined for scheduling purposes while a single control signal is derived for the set. In this case there would be a GeneratingUnit for each member of the set and an additional GeneratingUnit corresponding to the set."@en;
  rdfs:subClassOf cim:Equipment;
  rdfs:label "GeneratingUnit" .

cim:GeneratingUnit.ControlAreaGeneratingUnit a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "ControlArea specifications for this generating unit."@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range cim:ControlAreaGeneratingUnit;
  rdfs:label "ControlAreaGeneratingUnit";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:ControlAreaGeneratingUnit.GeneratingUnit .

cim:GeneratingUnit.RotatingMachine a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "A synchronous machine may operate as a generator and as such becomes a member of a generating unit."@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range cim:RotatingMachine;
  rdfs:label "RotatingMachine";
  cims:multiplicity cims:M:1..n;
  owl:inverseOf cim:RotatingMachine.GeneratingUnit .

cim:GeneratingUnit.maxEconomicP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Maximum high economic active power limit, that should not exceed the maximum operating active power limit."@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "maxEconomicP";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:GeneratingUnit.maxOperatingP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "This is the maximum operating active power limit the dispatcher can enter for this unit."@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "maxOperatingP";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:GeneratingUnit.minEconomicP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Low economic active power limit that shall be greater than or equal to the minimum operating active power limit."@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "minEconomicP";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:GeneratingUnit.minOperatingP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "This is the minimum operating active power limit the dispatcher can enter for this unit."@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "minOperatingP";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:GeneratingUnit.nominalP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """The nominal power of the generating unit.  Used to give precise meaning to percentage based attributes such as the governor speed change droop (governorSCD attribute).
The attribute shall be a positive value equal to or less than RotatingMachine.ratedS."""@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "nominalP";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:GeneratingUnit.normalPF a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Generating unit economic participation factor.  The sum of the participation factors across generating units does not have to sum to one.  It is used for representing distributed slack participation factor. The attribute shall be a positive value or zero."@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "normalPF";
  cims:multiplicity cims:M:1..1 .

cim:GeneratingUnit.ratedGrossMaxP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """The unit's gross rated maximum capacity (book value).
The attribute shall be a positive value."""@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "ratedGrossMaxP";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:GeneratingUnit.ratedNetMaxP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """The net rated maximum capacity determined by subtracting the auxiliary power used to operate the internal plant machinery from the rated gross maximum capacity.
The attribute shall be a positive value."""@en;
  rdfs:domain cim:GeneratingUnit;
  rdfs:range xsd:float;
  rdfs:label "ratedNetMaxP";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

