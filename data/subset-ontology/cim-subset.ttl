cim:ACLineSegment.x a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Positive sequence series reactance of the entire line section."@en;
  rdfs:domain cim:ACLineSegment;
  rdfs:range xsd:float;
  rdfs:label "x";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Reactance;
  qudt:hasUnit unit:OHM .

cim:ACLineSegment.x0 a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Zero sequence series reactance of the entire line section."@en;
  rdfs:domain cim:ACLineSegment;
  rdfs:range xsd:float;
  rdfs:label "x0";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Reactance;
  qudt:hasUnit unit:OHM .

cim:ActivePowerLimit a owl:Class;
  rdfs:comment "Limit on active power flow."@en;
  rdfs:subClassOf cim:OperationalLimit;
  rdfs:label "ActivePowerLimit" .

cim:ActivePowerLimit.normalValue a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The normal value of active power limit. The attribute shall be a positive value or zero."@en;
  rdfs:domain cim:ActivePowerLimit;
  rdfs:range xsd:float;
  rdfs:label "normalValue";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:ActivePowerLimit.value a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Value of active power limit. The attribute shall be a positive value or zero."@en;
  rdfs:domain cim:ActivePowerLimit;
  rdfs:range xsd:float;
  rdfs:label "value";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:Analog a owl:Class;
  rdfs:comment "Analog represents an analog Measurement."@en;
  rdfs:subClassOf cim:Measurement;
  rdfs:label "Analog" .

cim:Analog.AnalogValues a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The values connected to this measurement."@en;
  rdfs:domain cim:Analog;
  rdfs:range cim:AnalogValue;
  rdfs:label "AnalogValues";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:AnalogValue.Analog .

cim:Analog.positiveFlowIn a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "If true then this measurement is an active power, reactive power or current with the convention that a positive value measured at the Terminal means power is flowing into the related PowerSystemResource."@en;
  rdfs:domain cim:Analog;
  rdfs:range xsd:boolean;
  rdfs:label "positiveFlowIn";
  cims:multiplicity cims:M:0..1;
  cims:pragmatics """For BiddingZoneBorder,
\"true\" means the flow is from BiddingZoneOne to BiddingZoneTwo.
\"false\" means the flow is in the opposite direction""" .

cim:AnalogValue a owl:Class;
  rdfs:comment "AnalogValue represents an analog MeasurementValue."@en;
  rdfs:subClassOf cim:MeasurementValue;
  rdfs:label "AnalogValue" .

cim:AnalogValue.Analog a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Measurement to which this value is connected."@en;
  rdfs:domain cim:AnalogValue;
  rdfs:range cim:Analog;
  rdfs:label "Analog";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Analog.AnalogValues .

cim:Asset a owl:Class;
  rdfs:comment "Tangible resource of the utility, including power system equipment, various end devices, cabinets, buildings, etc. For electrical network equipment, the role of the asset is defined through PowerSystemResource and its subclasses, defined mainly in the Wires model (refer to IEC61970-301 and model package IEC61970::Wires). Asset description places emphasis on the physical characteristics of the equipment fulfilling that role."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "Asset" .

cim:Asset.AssetContainer a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Container of this asset."@en;
  rdfs:domain cim:Asset;
  rdfs:range cim:AssetContainer;
  rdfs:label "AssetContainer";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:AssetContainer.Assets .

cim:Asset.PowerSystemResources a owl:ObjectProperty;
  rdfs:comment "All power system resources used to electrically model this asset. For example, transformer asset is electrically modelled with a transformer and its windings and tap changer."@en;
  rdfs:domain cim:Asset;
  rdfs:range cim:PowerSystemResource;
  rdfs:label "PowerSystemResources";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:PowerSystemResource.Assets .

cim:Asset.inUseState a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Indication of whether asset is currently deployed (in use), ready to be put into use or not available for use."@en;
  rdfs:domain cim:Asset;
  rdfs:range cim:InUseStateKind;
  rdfs:label "inUseState";
  cims:multiplicity cims:M:0..1 .

cim:Asset.serialNumber a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Serial number of this asset."@en;
  rdfs:domain cim:Asset;
  rdfs:label "serialNumber";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:String .

cim:Asset.type a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Utility-specific classification of Asset and its subtypes, according to their corporate standards, practices, and existing IT systems (e.g., for management of assets, maintenance, work, outage, customers, etc.)."@en;
  rdfs:domain cim:Asset;
  rdfs:label "type";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:String .

cim:AssetContainer a owl:Class;
  rdfs:comment "Asset that is aggregation of other assets such as conductors, transformers, switchgear, land, fences, buildings, equipment, vehicles, etc."@en;
  rdfs:subClassOf cim:Asset;
  rdfs:label "AssetContainer" .

cim:AssetContainer.Assets a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "All assets within this container asset."@en;
  rdfs:domain cim:AssetContainer;
  rdfs:range cim:Asset;
  rdfs:label "Assets";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Asset.AssetContainer .

cim:AssetInfo a owl:Class;
  rdfs:comment """Set of attributes of an asset, representing typical datasheet information of a physical device that can be instantiated and shared in different data exchange contexts:
- as attributes of an asset instance (installed or in stock)
- as attributes of an asset model (product by a manufacturer)
- as attributes of a type asset (generic type of an asset as used in designs/extension planning)."""@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "AssetInfo" .

cim:AssetInfo.ProductAssetModel a owl:ObjectProperty, owl:InverseFunctionalProperty,
    owl:FunctionalProperty;
  rdfs:comment "Product asset model which conforms to this catalog asset type."@en;
  rdfs:domain cim:AssetInfo;
  rdfs:range cim:ProductAssetModel;
  rdfs:label "ProductAssetModel";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:ProductAssetModel.AssetInfo .

cim:AssetModelUsageKind a owl:Class;
  rdfs:comment "Usage for an asset model."@en;
  rdfs:label "AssetModelUsageKind" .

cim:AssetModelUsageKind.transmission a cim:AssetModelUsageKind;
  rdfs:comment "Asset model is intended for use in transmission network."@en;
  rdfs:label "transmission" .

cim:AssetOrganisationRole a owl:Class;
  rdfs:comment "Role an organisation plays with respect to asset."@en;
  rdfs:subClassOf cim:OrganisationRole;
  rdfs:label "AssetOrganisationRole" .

cim:AssetOwner a owl:Class;
  rdfs:comment "Owner of the asset."@en;
  rdfs:subClassOf cim:AssetOrganisationRole;
  rdfs:label "AssetOwner" .

cim:AsynchronousMachineKind a owl:Class;
  rdfs:comment "Kind of Asynchronous Machine."@en;
  rdfs:label "AsynchronousMachineKind" .

cim:AuxiliaryEquipment a owl:Class;
  rdfs:comment """AuxiliaryEquipment describe equipment that is not performing any primary functions but support for the equipment performing the primary function.
AuxiliaryEquipment is attached to primary equipment via an association with Terminal."""@en;
  rdfs:subClassOf cim:Equipment;
  rdfs:label "AuxiliaryEquipment" .

cim:AuxiliaryEquipment.Terminal a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The Terminal at the equipment where the AuxiliaryEquipment is attached."@en;
  rdfs:domain cim:AuxiliaryEquipment;
  rdfs:range cim:Terminal;
  rdfs:label "Terminal";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Terminal.AuxiliaryEquipment .

cim:BaseVoltage a owl:Class;
  rdfs:comment "Defines a system base voltage which is referenced."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "BaseVoltage" .

cim:BaseVoltage.ConductingEquipment a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "All conducting equipment with this base voltage.  Use only when there is no voltage level container used and only one base voltage applies.  For example, not used for transformers."@en;
  rdfs:domain cim:BaseVoltage;
  rdfs:range cim:ConductingEquipment;
  rdfs:label "ConductingEquipment";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:ConductingEquipment.BaseVoltage .

cim:BaseVoltage.TopologicalNode a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The topological nodes at the base voltage."@en;
  rdfs:domain cim:BaseVoltage;
  rdfs:range cim:TopologicalNode;
  rdfs:label "TopologicalNode";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:TopologicalNode.BaseVoltage .

cim:BaseVoltage.TransformerEnds a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Transformer ends at the base voltage.  This is essential for PU calculation."@en;
  rdfs:domain cim:BaseVoltage;
  rdfs:range cim:TransformerEnd;
  rdfs:label "TransformerEnds";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:TransformerEnd.BaseVoltage .

cim:BaseVoltage.VoltageLevel a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The voltage levels having this base voltage."@en;
  rdfs:domain cim:BaseVoltage;
  rdfs:range cim:VoltageLevel;
  rdfs:label "VoltageLevel";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:VoltageLevel.BaseVoltage .

cim:BaseVoltage.nominalVoltage a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The power system resource's base voltage.  Shall be a positive value and not zero."@en;
  rdfs:domain cim:BaseVoltage;
  rdfs:range xsd:float;
  rdfs:label "nominalVoltage";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.k;
  cim:unitSymbol cim:UnitSymbol.V;
  qudt:hasQuantityKind cim:Voltage;
  qudt:hasUnit unit:KiloV .

cim:BatteryStateKind a owl:Class;
  rdfs:comment "The state of the battery unit."@en;
  rdfs:label "BatteryStateKind" .

cim:Bay a owl:Class;
  rdfs:comment "A collection of power system resources (within a given substation) including conducting equipment, protection relays, measurements, and telemetry.  A bay typically represents a physical grouping related to modularization of equipment."@en;
  rdfs:subClassOf cim:EquipmentContainer;
  rdfs:label "Bay" .

cim:Bay.VoltageLevel a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The voltage level containing this bay."@en;
  rdfs:domain cim:Bay;
  rdfs:range cim:VoltageLevel;
  rdfs:label "VoltageLevel";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:VoltageLevel.Bays .

cim:Breaker a owl:Class;
  rdfs:comment "A mechanical switching device capable of making, carrying, and breaking currents under normal circuit conditions and also making, carrying for a specified time, and breaking currents under specified abnormal circuit conditions e.g.  those of short circuit."@en;
  rdfs:subClassOf cim:ProtectedSwitch;
  rdfs:label "Breaker" .

cim:BusbarSection a owl:Class;
  rdfs:comment """A conductor, or group of conductors, with negligible impedance, that serve to connect other conducting equipment within a single substation. 
Voltage measurements are typically obtained from voltage transformers that are connected to busbar sections. A bus bar section may have many physical terminals but for analysis is modelled with exactly one logical terminal."""@en;
  rdfs:subClassOf cim:Connector;
  rdfs:label "BusbarSection" .

cim:BusbarSection.ipMax a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Maximum allowable peak short-circuit current of busbar (Ipmax in IEC 60909-0). 
Mechanical limit of the busbar in the substation itself. Used for short circuit data exchange according to IEC 60909."""@en;
  rdfs:domain cim:BusbarSection;
  rdfs:range xsd:float;
  rdfs:label "ipMax";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.A;
  qudt:hasQuantityKind cim:CurrentFlow;
  qudt:hasUnit unit:A .

cim:CableInfo a owl:Class;
  rdfs:comment "Cable data."@en;
  rdfs:subClassOf cim:WireInfo;
  rdfs:label "CableInfo" .

cim:ConductingEquipment a owl:Class;
  rdfs:comment "The parts of the AC power system that are designed to carry current or that are conductively connected through terminals."@en;
  rdfs:subClassOf cim:Equipment;
  rdfs:label "ConductingEquipment";
  cims:pragmatics "This means only AC Conducting Equipment (not DC)" .

cim:ConductingEquipment.BaseVoltage a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Base voltage of this conducting equipment.  Use only when there is no voltage level container used and only one base voltage applies.  For example, not used for transformers."@en;
  rdfs:domain cim:ConductingEquipment;
  rdfs:range cim:BaseVoltage;
  rdfs:label "BaseVoltage";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:BaseVoltage.ConductingEquipment .

cim:ConductingEquipment.SvStatus a owl:ObjectProperty, owl:InverseFunctionalProperty,
    owl:FunctionalProperty;
  rdfs:comment "The status state variable associated with this conducting equipment."@en;
  rdfs:domain cim:ConductingEquipment;
  rdfs:range cim:SvStatus;
  rdfs:label "SvStatus";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:SvStatus.ConductingEquipment .

cim:ConductingEquipment.Terminals a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Conducting equipment have terminals that may be connected to other conducting equipment terminals via connectivity nodes or topological nodes."@en;
  rdfs:domain cim:ConductingEquipment;
  rdfs:range cim:Terminal;
  rdfs:label "Terminals";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Terminal.ConductingEquipment .

cim:Conductor a owl:Class;
  rdfs:comment "Combination of conducting material with consistent electrical characteristics, building a single electrical system, used to carry current between points in the power system."@en;
  rdfs:subClassOf cim:ConductingEquipment;
  rdfs:label "Conductor" .

cim:Conductor.length a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Segment length for calculating line section capabilities."@en;
  rdfs:domain cim:Conductor;
  rdfs:range xsd:float;
  rdfs:label "length";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.k;
  cim:unitSymbol cim:UnitSymbol.m;
  qudt:hasQuantityKind cim:Length;
  qudt:hasUnit unit:KiloM .

cim:ConformLoad a owl:Class;
  rdfs:comment "ConformLoad represent loads that follow a daily load change pattern where the pattern can be used to scale the load with a system load."@en;
  rdfs:subClassOf cim:EnergyConsumer;
  rdfs:label "ConformLoad" .

cim:ConformLoad.LoadGroup a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Group of this ConformLoad."@en;
  rdfs:domain cim:ConformLoad;
  rdfs:range cim:ConformLoadGroup;
  rdfs:label "LoadGroup";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:ConformLoadGroup.EnergyConsumers .

cim:ConformLoadGroup a owl:Class;
  rdfs:comment "A group of loads conforming to an allocation pattern."@en;
  rdfs:subClassOf cim:LoadGroup;
  rdfs:label "ConformLoadGroup" .

cim:ConformLoadGroup.EnergyConsumers a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Conform loads assigned to this ConformLoadGroup."@en;
  rdfs:domain cim:ConformLoadGroup;
  rdfs:range cim:ConformLoad;
  rdfs:label "EnergyConsumers";
  cims:multiplicity cims:M:1..n;
  owl:inverseOf cim:ConformLoad.LoadGroup .

cim:ConnectivityNode a owl:Class;
  rdfs:comment "Connectivity nodes are points where terminals of AC conducting equipment are connected together with zero impedance."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "ConnectivityNode" .

cim:ConnectivityNode.ConnectivityNodeContainer a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Container of this connectivity node."@en;
  rdfs:domain cim:ConnectivityNode;
  rdfs:range cim:ConnectivityNodeContainer;
  rdfs:label "ConnectivityNodeContainer";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:ConnectivityNodeContainer.ConnectivityNodes .

cim:ConnectivityNode.Terminals a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Terminals interconnected with zero impedance at a this connectivity node."@en;
  rdfs:domain cim:ConnectivityNode;
  rdfs:range cim:Terminal;
  rdfs:label "Terminals";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Terminal.ConnectivityNode .

cim:ConnectivityNode.TopologicalNode a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The topological node to which this connectivity node is assigned.  May depend on the current state of switches in the network."@en;
  rdfs:domain cim:ConnectivityNode;
  rdfs:range cim:TopologicalNode;
  rdfs:label "TopologicalNode";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:TopologicalNode.ConnectivityNodes .

cim:ConnectivityNodeContainer a owl:Class;
  rdfs:comment "A base class for all objects that may contain connectivity nodes or topological nodes."@en;
  rdfs:subClassOf cimr:EquipmentOrContainer;
  rdfs:label "ConnectivityNodeContainer" .

cim:ConnectivityNodeContainer.ConnectivityNodes a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Connectivity nodes which belong to this connectivity node container."@en;
  rdfs:domain cim:ConnectivityNodeContainer;
  rdfs:range cim:ConnectivityNode;
  rdfs:label "ConnectivityNodes";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:ConnectivityNode.ConnectivityNodeContainer .

cim:ConnectivityNodeContainer.TopologicalNode a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The topological nodes which belong to this connectivity node container."@en;
  rdfs:domain cim:ConnectivityNodeContainer;
  rdfs:range cim:TopologicalNode;
  rdfs:label "TopologicalNode";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:TopologicalNode.ConnectivityNodeContainer .

cim:Connector a owl:Class;
  rdfs:comment "A conductor, or group of conductors, with negligible impedance, that serve to connect other conducting equipment within a single substation and are modelled with a single logical terminal."@en;
  rdfs:subClassOf cim:ConductingEquipment;
  rdfs:label "Connector" .

cim:Contingency a owl:Class;
  rdfs:comment "An event threatening system reliability, consisting of one or more contingency elements."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "Contingency" .

cim:Contingency.ContingencyElement a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "A contingency can have any number of contingency elements."@en;
  rdfs:domain cim:Contingency;
  rdfs:range cim:ContingencyElement;
  rdfs:label "ContingencyElement";
  cims:multiplicity cims:M:1..n;
  owl:inverseOf cim:ContingencyElement.Contingency .

cim:Contingency.mustStudy a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Set true if must study this contingency."@en;
  rdfs:domain cim:Contingency;
  rdfs:range xsd:boolean;
  rdfs:label "mustStudy";
  cims:multiplicity cims:M:1..1 .

cim:ContingencyElement a owl:Class;
  rdfs:comment "An element of a system event to be studied by contingency analysis, representing a change in status of a single piece of equipment."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "ContingencyElement" .

cim:ContingencyElement.Contingency a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "A contingency element belongs to one contingency."@en;
  rdfs:domain cim:ContingencyElement;
  rdfs:range cim:Contingency;
  rdfs:label "Contingency";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Contingency.ContingencyElement .

cim:ContingencyEquipment a owl:Class;
  rdfs:comment "Equipment whose in service status is to change, such as a power transformer or AC line segment."@en;
  rdfs:subClassOf cim:ContingencyElement;
  rdfs:label "ContingencyEquipment" .

cim:ContingencyEquipment.Equipment a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The single piece of equipment to which to apply the contingency."@en;
  rdfs:domain cim:ContingencyEquipment;
  rdfs:range cim:Equipment;
  rdfs:label "Equipment";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Equipment.ContingencyEquipment .

cim:ContingencyEquipment.contingentStatus a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The status for the associated equipment when in the contingency state.   This status is independent of the case to which the contingency is originally applied, but defines the equipment status when the contingency is applied."@en;
  rdfs:domain cim:ContingencyEquipment;
  rdfs:range cim:ContingencyEquipmentStatusKind;
  rdfs:label "contingentStatus";
  cims:multiplicity cims:M:1..1 .

cim:ContingencyEquipmentStatusKind a owl:Class;
  rdfs:comment "Indicates the state which the contingency equipment is to be in when the contingency is applied."@en;
  rdfs:label "ContingencyEquipmentStatusKind" .

cim:ContingencyEquipmentStatusKind.outOfService a cim:ContingencyEquipmentStatusKind;
  rdfs:comment "The equipment is to be taken out of service."@en;
  rdfs:label "outOfService" .

cim:ControlArea a owl:Class;
  rdfs:comment "A control area is a grouping of generating units and/or loads and a cutset of tie lines (as terminals) which may be used for a variety of purposes including automatic generation control, power flow solution area interchange control specification, and input to load forecasting. All generation and load within the area defined by the terminals on the border are considered in the area interchange control. Note that any number of overlapping control area specifications can be superimposed on the physical"@en;
  rdfs:subClassOf cim:PowerSystemResource;
  rdfs:label "ControlArea" .

cim:ControlArea.ControlAreaGeneratingUnit a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The generating unit specifications for the control area."@en;
  rdfs:domain cim:ControlArea;
  rdfs:range cim:ControlAreaGeneratingUnit;
  rdfs:label "ControlAreaGeneratingUnit";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:ControlAreaGeneratingUnit.ControlArea .

cim:ControlArea.EnergyArea a owl:ObjectProperty, owl:InverseFunctionalProperty, owl:FunctionalProperty;
  rdfs:comment "The energy area that is forecast from this control area specification."@en;
  rdfs:domain cim:ControlArea;
  rdfs:range cim:EnergyArea;
  rdfs:label "EnergyArea";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:EnergyArea.ControlArea .

cim:ControlArea.netInterchange a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The specified positive net interchange into the control area, i.e. positive sign means flow into the area."@en;
  rdfs:domain cim:ControlArea;
  rdfs:range xsd:float;
  rdfs:label "netInterchange";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:ControlArea.pTolerance a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Active power net interchange tolerance. The attribute shall be a positive value or zero."@en;
  rdfs:domain cim:ControlArea;
  rdfs:range xsd:float;
  rdfs:label "pTolerance";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:ControlArea.type a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The primary type of control area definition used to determine if this is used for automatic generation control, for planning interchange control, or other purposes.   A control area specified with primary type of automatic generation control could still be forecast and used as an interchange area in power flow analysis."@en;
  rdfs:domain cim:ControlArea;
  rdfs:range cim:ControlAreaTypeKind;
  rdfs:label "type";
  cims:multiplicity cims:M:1..1 .

cim:ControlAreaGeneratingUnit a owl:Class;
  rdfs:comment "A control area generating unit. This class is needed so that alternate control area definitions may include the same generating unit.   It should be noted that only one instance within a control area should reference a specific generating unit."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "ControlAreaGeneratingUnit" .

cim:ControlAreaGeneratingUnit.ControlArea a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The parent control area for the generating unit specifications."@en;
  rdfs:domain cim:ControlAreaGeneratingUnit;
  rdfs:range cim:ControlArea;
  rdfs:label "ControlArea";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:ControlArea.ControlAreaGeneratingUnit .

cim:ControlAreaGeneratingUnit.GeneratingUnit a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The generating unit specified for this control area.  Note that a control area should include a GeneratingUnit only once."@en;
  rdfs:domain cim:ControlAreaGeneratingUnit;
  rdfs:range cim:GeneratingUnit;
  rdfs:label "GeneratingUnit";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:GeneratingUnit.ControlAreaGeneratingUnit .

cim:ControlAreaTypeKind a owl:Class;
  rdfs:comment "The type of control area."@en;
  rdfs:label "ControlAreaTypeKind" .

cim:ControlAreaTypeKind.Interchange a cim:ControlAreaTypeKind;
  rdfs:comment "Used for interchange specification or control."@en;
  rdfs:label "Interchange" .

cim:CoordinateSystem a owl:Class;
  rdfs:comment "Coordinate reference system."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "CoordinateSystem" .

cim:CoordinateSystem.Locations a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "All locations described with position points in this coordinate system."@en;
  rdfs:domain cim:CoordinateSystem;
  rdfs:range cim:Location;
  rdfs:label "Locations";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Location.CoordinateSystem .

cim:CoordinateSystem.crsUrn a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "A Uniform Resource Name (URN) for the coordinate reference system (crs) used to define 'Location.PositionPoints'. An example would be the European Petroleum Survey Group (EPSG) code for a coordinate reference system, defined in URN under the Open Geospatial Consortium (OGC) namespace as: urn:ogc:def:crs:EPSG::XXXX, where XXXX is an EPSG code (a full list of codes can be found at the EPSG Registry web site http://www.epsg-registry.org/). To define the coordinate system as being WGS84 (latitude, longitude)"@en;
  rdfs:domain cim:CoordinateSystem;
  rdfs:range xsd:string;
  rdfs:label "crsUrn";
  cims:multiplicity cims:M:1..1 .

cim:CsOperatingModeKind a owl:Class;
  rdfs:comment "Operating mode for HVDC line operating as Current Source Converter."@en;
  rdfs:label "CsOperatingModeKind" .

cim:CsPpccControlKind a owl:Class;
  rdfs:comment "Active power control modes for HVDC line operating as Current Source Converter."@en;
  rdfs:label "CsPpccControlKind" .

cim:Currency a owl:Class;
  rdfs:comment "Monetary currencies.  ISO 4217 standard including 3-character currency code."@en;
  rdfs:label "Currency" .

cim:CurrentLimit a owl:Class;
  rdfs:comment "Operational limit on current."@en;
  rdfs:subClassOf cim:OperationalLimit;
  rdfs:label "CurrentLimit" .

cim:CurrentLimit.normalValue a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The normal value for limit on current flow. The attribute shall be a positive value or zero."@en;
  rdfs:domain cim:CurrentLimit;
  rdfs:range xsd:float;
  rdfs:label "normalValue";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.A;
  qudt:hasQuantityKind cim:CurrentFlow;
  qudt:hasUnit unit:A .

cim:CurrentLimit.value a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Limit on current flow. The attribute shall be a positive value or zero."@en;
  rdfs:domain cim:CurrentLimit;
  rdfs:range xsd:float;
  rdfs:label "value";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.A;
  qudt:hasQuantityKind cim:CurrentFlow;
  qudt:hasUnit unit:A .

cim:CurrentTransformer a owl:Class;
  rdfs:comment "Instrument transformer used to measure electrical qualities of the circuit that is being protected and/or monitored. Typically used as current transducer for the purpose of metering or protection. A typical secondary current rating would be 5A."@en;
  rdfs:subClassOf cim:Sensor;
  rdfs:label "CurrentTransformer" .

cim:Curve a owl:Class;
  rdfs:comment "A multi-purpose curve or functional relationship between an independent variable (X-axis) and dependent (Y-axis) variables."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "Curve" .

cim:Curve.CurveDatas a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The point data values that define this curve."@en;
  rdfs:domain cim:Curve;
  rdfs:range cim:CurveData;
  rdfs:label "CurveDatas";
  cims:multiplicity cims:M:1..n;
  owl:inverseOf cim:CurveData.Curve .

cim:Curve.curveStyle a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The style or shape of the curve."@en;
  rdfs:domain cim:Curve;
  rdfs:range cim:CurveStyle;
  rdfs:label "curveStyle";
  cims:multiplicity cims:M:1..1 .

cim:Curve.xUnit a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The X-axis units of measure."@en;
  rdfs:domain cim:Curve;
  rdfs:range cim:UnitSymbol;
  rdfs:label "xUnit";
  cims:multiplicity cims:M:1..1 .

cim:Curve.y2Unit a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The Y2-axis units of measure."@en;
  rdfs:domain cim:Curve;
  rdfs:range cim:UnitSymbol;
  rdfs:label "y2Unit";
  cims:multiplicity cims:M:0..1 .

cim:CurveData a owl:Class;
  rdfs:comment "Multi-purpose data points for defining a curve.  The use of this generic class is discouraged if a more specific class can be used to specify the X and Y axis values along with their specific data types."@en;
  rdfs:label "CurveData" .

cim:CurveData.Curve a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The curve of  this curve data point."@en;
  rdfs:domain cim:CurveData;
  rdfs:range cim:Curve;
  rdfs:label "Curve";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Curve.CurveDatas .

cim:CurveStyle a owl:Class;
  rdfs:comment "Style or shape of curve."@en;
  rdfs:label "CurveStyle" .

cim:DCConverterOperatingModeKind a owl:Class;
  rdfs:comment "The operating mode of an HVDC bipole."@en;
  rdfs:label "DCConverterOperatingModeKind" .

cim:DCPolarityKind a owl:Class;
  rdfs:comment "Polarity for DC circuits."@en;
  rdfs:label "DCPolarityKind" .

cim:Diagram a owl:Class;
  rdfs:comment "The diagram being exchanged. The coordinate system is a standard Cartesian coordinate system and the orientation attribute defines the orientation. The initial view related attributes can be used to specify an initial view with the x,y coordinates of the diagonal points."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "Diagram" .

cim:Diagram.DiagramElements a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "A diagram is made up of multiple diagram objects."@en;
  rdfs:domain cim:Diagram;
  rdfs:range cim:DiagramObject;
  rdfs:label "DiagramElements";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:DiagramObject.Diagram .

cim:Diagram.orientation a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Coordinate system orientation of the diagram. A positive orientation gives standard “right-hand” orientation, with negative orientation indicating a “left-hand” orientation. For 2D diagrams, a positive orientation will result in X values increasing from left to right and Y values increasing from bottom to top. A negative orientation gives the “left-hand” orientation (favoured by computer graphics displays) with X values increasing from left to right and Y values increasing from top to bottom."@en;
  rdfs:domain cim:Diagram;
  rdfs:range cim:OrientationKind;
  rdfs:label "orientation";
  cims:multiplicity cims:M:1..1 .

cim:DiagramObject a owl:Class;
  rdfs:comment "An object that defines one or more points in a given space. This object can be associated with anything that specializes IdentifiedObject. For single line diagrams such objects typically include such items as analog values, breakers, disconnectors, power transformers, and transmission lines."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "DiagramObject" .

cim:DiagramObject.Diagram a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "A diagram object is part of a diagram."@en;
  rdfs:domain cim:DiagramObject;
  rdfs:range cim:Diagram;
  rdfs:label "Diagram";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Diagram.DiagramElements .

cim:DiagramObject.DiagramObjectPoints a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "A diagram object can have 0 or more points to reflect its layout position, routing (for polylines) or boundary (for polygons)."@en;
  rdfs:domain cim:DiagramObject;
  rdfs:range cim:DiagramObjectPoint;
  rdfs:label "DiagramObjectPoints";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:DiagramObjectPoint.DiagramObject .

cim:DiagramObject.DiagramObjectStyle a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "A diagram object has a style associated that provides a reference for the style used in the originating system."@en;
  rdfs:domain cim:DiagramObject;
  rdfs:range cim:DiagramObjectStyle;
  rdfs:label "DiagramObjectStyle";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:DiagramObjectStyle.StyledObjects .

cim:DiagramObject.IdentifiedObject a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The domain object to which this diagram object is associated."@en;
  rdfs:domain cim:DiagramObject;
  rdfs:range cim:IdentifiedObject;
  rdfs:label "IdentifiedObject";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:IdentifiedObject.DiagramObjects .

cim:DiagramObject.rotation a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Sets the angle of rotation of the diagram object.  Zero degrees is pointing to the top of the diagram.  Rotation is clockwise.  DiagramObject.rotation=0 has the following meaning: The connection point of an element which has one terminal is pointing to the top side of the diagram. The connection point \"From side\" of an element which has more than one terminal is pointing to the top side of the diagram. DiagramObject.rotation=90 has the following meaning: The connection point of an element which has"@en;
  rdfs:domain cim:DiagramObject;
  rdfs:range xsd:float;
  rdfs:label "rotation";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.deg;
  qudt:hasQuantityKind cim:AngleDegrees;
  qudt:hasUnit unit:DEG .

cim:DiagramObjectPoint a owl:Class;
  rdfs:comment "A point in a given space defined by 3 coordinates and associated to a diagram object.  The coordinates may be positive or negative as the origin does not have to be in the corner of a diagram."@en;
  rdfs:label "DiagramObjectPoint" .

cim:DiagramObjectPoint.DiagramObject a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The diagram object with which the points are associated."@en;
  rdfs:domain cim:DiagramObjectPoint;
  rdfs:range cim:DiagramObject;
  rdfs:label "DiagramObject";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:DiagramObject.DiagramObjectPoints .

cim:DiagramObjectPoint.sequenceNumber a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The sequence position of the point, used for defining the order of points for diagram objects acting as a polyline or polygon with more than one point. The attribute shall be a positive value."@en;
  rdfs:domain cim:DiagramObjectPoint;
  rdfs:range xsd:integer;
  rdfs:label "sequenceNumber";
  cims:multiplicity cims:M:0..1 .

cim:DiagramObjectPoint.xPosition a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The X coordinate of this point."@en;
  rdfs:domain cim:DiagramObjectPoint;
  rdfs:range xsd:float;
  rdfs:label "xPosition";
  cims:multiplicity cims:M:1..1 .

cim:DiagramObjectPoint.yPosition a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The Y coordinate of this point."@en;
  rdfs:domain cim:DiagramObjectPoint;
  rdfs:range xsd:float;
  rdfs:label "yPosition";
  cims:multiplicity cims:M:1..1 .

cim:DiagramObjectStyle a owl:Class;
  rdfs:comment "A reference to a style used by the originating system for a diagram object.  A diagram object style describes information such as line thickness, shape such as circle or rectangle etc, and colour."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "DiagramObjectStyle" .

cim:DiagramObjectStyle.StyledObjects a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "A style can be assigned to multiple diagram objects."@en;
  rdfs:domain cim:DiagramObjectStyle;
  rdfs:range cim:DiagramObject;
  rdfs:label "StyledObjects";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:DiagramObject.DiagramObjectStyle .

cim:Disconnector a owl:Class;
  rdfs:comment "A manually operated or motor operated mechanical switching device used for changing the connections in a circuit, or for isolating a circuit or equipment from a source of power. It is required to open or close circuits when negligible current is broken or made."@en;
  rdfs:subClassOf cim:Switch;
  rdfs:label "Disconnector" .

cim:Discrete a owl:Class;
  rdfs:comment "Discrete represents a discrete Measurement, i.e. a Measurement representing discrete values, e.g. a Breaker position."@en;
  rdfs:subClassOf cim:Measurement;
  rdfs:label "Discrete" .

cim:Discrete.DiscreteValues a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The values connected to this measurement."@en;
  rdfs:domain cim:Discrete;
  rdfs:range cim:DiscreteValue;
  rdfs:label "DiscreteValues";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:DiscreteValue.Discrete .

cim:DiscreteValue a owl:Class;
  rdfs:comment "DiscreteValue represents a discrete MeasurementValue."@en;
  rdfs:subClassOf cim:MeasurementValue;
  rdfs:label "DiscreteValue" .

cim:DiscreteValue.Discrete a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Measurement to which this value is connected."@en;
  rdfs:domain cim:DiscreteValue;
  rdfs:range cim:Discrete;
  rdfs:label "Discrete";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:Discrete.DiscreteValues .

cim:DroopSignalFeedbackKind a owl:Class;
  rdfs:comment "Governor droop signal feedback source."@en;
  rdfs:label "DroopSignalFeedbackKind" .

cim:EarthFaultCompensator a owl:Class;
  rdfs:comment "A conducting equipment used to represent a connection to ground which is typically used to compensate earth faults.   An earth fault compensator device modelled with a single terminal implies a second terminal solidly connected to ground.  If two terminals are modelled, the ground is not assumed and normal connection rules apply."@en;
  rdfs:subClassOf cim:ConductingEquipment;
  rdfs:label "EarthFaultCompensator" .

cim:EndDevice a owl:Class;
  rdfs:comment "Asset container that performs one or more end device functions. One type of end device is a meter which can perform metering, load management, connect/disconnect, accounting functions, etc. Some end devices, such as ones monitoring and controlling air conditioners, refrigerators, pool pumps may be connected to a meter. All end devices may have communication capability defined by the associated communication function(s). An end device may be owned by a consumer, a service provider, utility or otherwise."@en;
  rdfs:subClassOf cim:AssetContainer;
  rdfs:label "EndDevice" .

cim:EnergyArea a owl:Class;
  rdfs:comment "Describes an area having energy production or consumption.  Specializations are intended to support the load allocation function as typically required in energy management systems or planning studies to allocate hypothesized load levels to individual load points for power flow analysis.  Often the energy area can be linked to both measured and forecast load levels."@en;
  rdfs:subClassOf cim:IdentifiedObject;
  rdfs:label "EnergyArea" .

cim:EnergyArea.ControlArea a owl:ObjectProperty, owl:InverseFunctionalProperty, owl:FunctionalProperty;
  rdfs:comment "The control area specification that is used for the load forecast."@en;
  rdfs:domain cim:EnergyArea;
  rdfs:range cim:ControlArea;
  rdfs:label "ControlArea";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:ControlArea.EnergyArea .

cim:EnergyConnection a owl:Class;
  rdfs:comment "A connection of energy generation or consumption on the power system model."@en;
  rdfs:subClassOf cim:ConductingEquipment;
  rdfs:label "EnergyConnection" .

cim:EnergyConsumer a owl:Class;
  rdfs:comment """Generic user of energy - a  point of consumption on the power system model.
EnergyConsumer.pfixed, .qfixed, .pfixedPct and .qfixedPct have meaning only if there is no LoadResponseCharacteristic associated with EnergyConsumer or if LoadResponseCharacteristic.exponentModel is set to False."""@en;
  rdfs:subClassOf cim:EnergyConnection;
  rdfs:label "EnergyConsumer" .

cim:EnergyConsumer.LoadResponse a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The load response characteristic of this load.  If missing, this load is assumed to be constant power."@en;
  rdfs:domain cim:EnergyConsumer;
  rdfs:range cim:LoadResponseCharacteristic;
  rdfs:label "LoadResponse";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:LoadResponseCharacteristic.EnergyConsumer .

cim:EnergyConsumer.p a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Active power of the load. Load sign convention is used, i.e. positive sign means flow out from a node.
For voltage dependent loads the value is at rated voltage.
Starting value for a steady state solution."""@en;
  rdfs:domain cim:EnergyConsumer;
  rdfs:range xsd:float;
  rdfs:label "p";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:EnergyConsumer.pfixed a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Active power of the load that is a fixed quantity and does not vary as load group value varies. Load sign convention is used, i.e. positive sign means flow out from a node."@en;
  rdfs:domain cim:EnergyConsumer;
  rdfs:range xsd:float;
  rdfs:label "pfixed";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:EnergyConsumer.pfixedPct a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Fixed active power as a percentage of load group fixed active power. Used to represent the time-varying components.  Load sign convention is used, i.e. positive sign means flow out from a node."@en;
  rdfs:domain cim:EnergyConsumer;
  rdfs:range xsd:float;
  rdfs:label "pfixedPct";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.none;
  qudt:hasQuantityKind cim:PerCent;
  qudt:hasUnit unit:PERCENT .

cim:EnergyConsumer.q a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Reactive power of the load. Load sign convention is used, i.e. positive sign means flow out from a node.
For voltage dependent loads the value is at rated voltage.
Starting value for a steady state solution."""@en;
  rdfs:domain cim:EnergyConsumer;
  rdfs:range xsd:float;
  rdfs:label "q";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:EnergyConsumer.qfixed a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Reactive power of the load that is a fixed quantity and does not vary as load group value varies. Load sign convention is used, i.e. positive sign means flow out from a node."@en;
  rdfs:domain cim:EnergyConsumer;
  rdfs:range xsd:float;
  rdfs:label "qfixed";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:EnergyConsumer.qfixedPct a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Fixed reactive power as a percentage of load group fixed reactive power. Used to represent the time-varying components.  Load sign convention is used, i.e. positive sign means flow out from a node."@en;
  rdfs:domain cim:EnergyConsumer;
  rdfs:range xsd:float;
  rdfs:label "qfixedPct";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.none;
  qudt:hasQuantityKind cim:PerCent;
  qudt:hasUnit unit:PERCENT .

cim:Equipment a owl:Class;
  rdfs:comment "The parts of a power system that are physical devices, electronic or mechanical."@en;
  rdfs:subClassOf cimr:EquipmentOrContainer;
  rdfs:label "Equipment" .

cim:Equipment.ContingencyEquipment a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The contingency equipments in which this equipment participates."@en;
  rdfs:domain cim:Equipment;
  rdfs:range cim:ContingencyEquipment;
  rdfs:label "ContingencyEquipment";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:ContingencyEquipment.Equipment .

cim:Equipment.EquipmentContainer a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Container of this equipment."@en;
  rdfs:domain cim:Equipment;
  rdfs:range cim:EquipmentContainer;
  rdfs:label "EquipmentContainer";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:EquipmentContainer.Equipments .

cim:Equipment.aggregate a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The aggregate flag provides an alternative way of representing an aggregated (equivalent) element. It is applicable in cases when the dedicated classes for equivalent equipment do not have all of the attributes necessary to represent the required level of detail.  In case the flag is set to “true” the single instance of equipment represents multiple pieces of equipment that have been modelled together as an aggregate equivalent obtained by a network reduction procedure. Examples would be power transformers"@en;
  rdfs:domain cim:Equipment;
  rdfs:range xsd:boolean;
  rdfs:label "aggregate";
  cims:multiplicity cims:M:0..1 .

cim:Equipment.inService a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Specifies the availability of the equipment. True means the equipment is available for topology processing, which determines if the equipment is energized or not. False means that the equipment is treated by network applications as if it is not in the model."@en;
  rdfs:domain cim:Equipment;
  rdfs:range xsd:boolean;
  rdfs:label "inService";
  cims:multiplicity cims:M:1..1 .

cim:Equipment.networkAnalysisEnabled a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "The equipment is enabled to participate in network analysis.  If unspecified, the value is assumed to be true."@en;
  rdfs:domain cim:Equipment;
  rdfs:range xsd:boolean;
  rdfs:label "networkAnalysisEnabled";
  cims:multiplicity cims:M:0..1 .

cim:Equipment.normallyInService a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Specifies the availability of the equipment under normal operating conditions. True means the equipment is available for topology processing, which determines if the equipment is energized or not. False means that the equipment is treated by network applications as if it is not in the model."@en;
  rdfs:domain cim:Equipment;
  rdfs:range xsd:boolean;
  rdfs:label "normallyInService";
  cims:multiplicity cims:M:0..1 .

cim:EquipmentContainer a owl:Class;
  rdfs:comment "A modelling construct to provide a root class for containing equipment."@en;
  rdfs:subClassOf cim:ConnectivityNodeContainer;
  rdfs:label "EquipmentContainer" .

cim:EquipmentContainer.Equipments a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "Contained equipment."@en;
  rdfs:subPropertyOf cimr:hasPart;
  rdfs:domain cim:EquipmentContainer;
  rdfs:range cim:Equipment;
  rdfs:label "Equipments";
  cims:multiplicity cims:M:0..n;
  owl:inverseOf cim:Equipment.EquipmentContainer .

cim:EquivalentEquipment a owl:Class;
  rdfs:comment "The class represents equivalent objects that are the result of a network reduction. The class is the base for equivalent objects of different types."@en;
  rdfs:subClassOf cim:ConductingEquipment;
  rdfs:label "EquivalentEquipment" .

cim:EquivalentInjection a owl:Class;
  rdfs:comment "This class represents equivalent injections (generation or load).  Voltage regulation is allowed only at the point of connection."@en;
  rdfs:subClassOf cim:EquivalentEquipment;
  rdfs:label "EquivalentInjection" .

cim:EquivalentInjection.maxP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Maximum active power of the injection."@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "maxP";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:EquivalentInjection.maxQ a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Maximum reactive power of the injection.  Used for modelling of infeed for load flow exchange. Not used for short circuit modelling.  If maxQ and minQ are not used ReactiveCapabilityCurve can be used."@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "maxQ";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:EquivalentInjection.minP a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Minimum active power of the injection."@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "minP";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:EquivalentInjection.minQ a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Minimum reactive power of the injection.  Used for modelling of infeed for load flow exchange. Not used for short circuit modelling.  If maxQ and minQ are not used ReactiveCapabilityCurve can be used."@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "minQ";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:EquivalentInjection.p a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Equivalent active power injection. Load sign convention is used, i.e. positive sign means flow out from a node.
Starting value for steady state solutions."""@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "p";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.W;
  qudt:hasQuantityKind cim:ActivePower;
  qudt:hasUnit unit:MegaW .

cim:EquivalentInjection.q a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Equivalent reactive power injection. Load sign convention is used, i.e. positive sign means flow out from a node.
Starting value for steady state solutions."""@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "q";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:EquivalentInjection.r a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Positive sequence resistance. Used to represent Extended-Ward (IEC 60909).
Usage : Extended-Ward is a result of network reduction prior to the data exchange."""@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "r";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Resistance;
  qudt:hasUnit unit:OHM .

cim:EquivalentInjection.r0 a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Zero sequence resistance. Used to represent Extended-Ward (IEC 60909).
Usage : Extended-Ward is a result of network reduction prior to the data exchange."""@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "r0";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Resistance;
  qudt:hasUnit unit:OHM .

cim:EquivalentInjection.regulationCapability a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Specifies whether or not the EquivalentInjection has the capability to regulate the local voltage. If true the EquivalentInjection can regulate. If false the EquivalentInjection cannot regulate. ReactiveCapabilityCurve can only be associated with EquivalentInjection  if the flag is true."@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:boolean;
  rdfs:label "regulationCapability";
  cims:multiplicity cims:M:1..1 .

cim:EquivalentInjection.x a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Positive sequence reactance. Used to represent Extended-Ward (IEC 60909).
Usage : Extended-Ward is a result of network reduction prior to the data exchange."""@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "x";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Reactance;
  qudt:hasUnit unit:OHM .

cim:EquivalentInjection.x0 a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Zero sequence reactance. Used to represent Extended-Ward (IEC 60909).
Usage : Extended-Ward is a result of network reduction prior to the data exchange."""@en;
  rdfs:domain cim:EquivalentInjection;
  rdfs:range xsd:float;
  rdfs:label "x0";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Reactance;
  qudt:hasUnit unit:OHM .

cim:ExcIEEEST1AUELselectorKind a owl:Class;
  rdfs:comment "Types of connections for the UEL input used in ExcIEEEST1A."@en;
  rdfs:label "ExcIEEEST1AUELselectorKind" .

cim:ExcREXSFeedbackSignalKind a owl:Class;
  rdfs:comment "Types of rate feedback signals."@en;
  rdfs:label "ExcREXSFeedbackSignalKind" .

cim:ExcST6BOELselectorKind a owl:Class;
  rdfs:comment "Types of connections for the OEL input used for static excitation systems type 6B."@en;
  rdfs:label "ExcST6BOELselectorKind" .

cim:ExcST7BOELselectorKind a owl:Class;
  rdfs:comment "Types of connections for the OEL input used for static excitation systems type 7B."@en;
  rdfs:label "ExcST7BOELselectorKind" .

cim:ExcST7BUELselectorKind a owl:Class;
