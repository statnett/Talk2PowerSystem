  rdfs:domain cim:Terminal;
  rdfs:range cim:ConductingEquipment;
  rdfs:label "ConductingEquipment";
  cims:multiplicity cims:M:1..1;
  owl:inverseOf cim:ConductingEquipment.Terminals .

cim:Terminal.ConnectivityNode a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The connectivity node to which this terminal connects with zero impedance."@en;
