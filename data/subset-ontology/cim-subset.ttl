  rdfs:label "maxQ";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:SynchronousMachine.minQ a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Minimum reactive power limit for the unit."@en;
  rdfs:domain cim:SynchronousMachine;
  rdfs:range xsd:float;
  rdfs:label "minQ";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.M;
  cim:unitSymbol cim:UnitSymbol.VAr;
  qudt:hasQuantityKind cim:ReactivePower;
  qudt:hasUnit unit:MegaV-A_Reactive .

cim:SynchronousMachine.operatingMode a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Current mode of operation."@en;
  rdfs:domain cim:SynchronousMachine;
  rdfs:range cim:SynchronousMachineOperatingMode;
  rdfs:label "operatingMode";
  cims:multiplicity cims:M:1..1 .

cim:SynchronousMachine.qPercent a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Part of the coordinated reactive control that comes from this machine. The attribute is used as a participation factor not necessarily summing up to 100% for the participating devices in the control."@en;
  rdfs:domain cim:SynchronousMachine;
  rdfs:range xsd:float;
  rdfs:label "qPercent";
  cims:multiplicity cims:M:0..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.none;
  qudt:hasQuantityKind cim:PerCent;
  qudt:hasUnit unit:PERCENT .

cim:SynchronousMachine.r a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Equivalent resistance (RG) of generator. RG is considered for the calculation of all currents, except for the calculation of the peak current ip. Used for short circuit data exchange according to IEC 60909."@en;
  rdfs:domain cim:SynchronousMachine;
  rdfs:range xsd:float;
  rdfs:label "r";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.none;
  cim:unitSymbol cim:UnitSymbol.ohm;
  qudt:hasQuantityKind cim:Resistance;
  qudt:hasUnit unit:OHM .

cim:SynchronousMachine.referencePriority a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Priority of unit for use as powerflow voltage phase angle reference bus selection. 0 = don t care (default) 1 = highest priority. 2 is less than 1 and so on."@en;
  rdfs:domain cim:SynchronousMachine;
  rdfs:range xsd:integer;
  rdfs:label "referencePriority";
  cims:multiplicity cims:M:1..1 .

cim:SynchronousMachine.type a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Modes that this synchronous machine can operate in."@en;
  rdfs:domain cim:SynchronousMachine;
  rdfs:range cim:SynchronousMachineKind;
  rdfs:label "type";
  cims:multiplicity cims:M:1..1 .

cim:SynchronousMachineKind a owl:Class;
  rdfs:comment "Synchronous machine type."@en;
  rdfs:label "SynchronousMachineKind" .

cim:SynchronousMachineKind.generator a cim:SynchronousMachineKind;
  rdfs:comment "Indicates the synchronous machine can operate as a generator."@en;
  rdfs:label "generator" .

cim:SynchronousMachineModelKind a owl:Class;
  rdfs:comment "Type of synchronous machine model used in dynamic simulation applications."@en;
  rdfs:label "SynchronousMachineModelKind" .

cim:SynchronousMachineOperatingMode a owl:Class;
  rdfs:comment "Synchronous machine operating mode."@en;
  rdfs:label "SynchronousMachineOperatingMode" .

cim:SynchronousMachineOperatingMode.generator a cim:SynchronousMachineOperatingMode;
  rdfs:comment "Operating as generator."@en;
  rdfs:label "generator" .

cim:TapChanger a owl:Class;
  rdfs:comment "Mechanism for changing transformer winding tap positions."@en;
  rdfs:subClassOf cim:PowerSystemResource;
  rdfs:label "TapChanger" .

cim:TapChanger.SvTapStep a owl:ObjectProperty, owl:InverseFunctionalProperty, owl:FunctionalProperty;
  rdfs:comment "The tap step state associated with the tap changer."@en;
  rdfs:domain cim:TapChanger;
  rdfs:range cim:SvTapStep;
  rdfs:label "SvTapStep";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:SvTapStep.TapChanger .

cim:TapChanger.TapChangerControl a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "The regulating control scheme in which this tap changer participates."@en;
  rdfs:domain cim:TapChanger;
  rdfs:range cim:TapChangerControl;
  rdfs:label "TapChangerControl";
  cims:multiplicity cims:M:0..1;
  owl:inverseOf cim:TapChangerControl.TapChanger .

cim:TapChanger.controlEnabled a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Specifies the regulation status of the equipment.  True is regulating, false is not regulating."@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:boolean;
  rdfs:label "controlEnabled";
  cims:multiplicity cims:M:1..1 .

cim:TapChanger.highStep a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Highest possible tap step position, advance from neutral.
The attribute shall be greater than lowStep."""@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:integer;
  rdfs:label "highStep";
  cims:multiplicity cims:M:1..1 .

cim:TapChanger.lowStep a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Lowest possible tap step position, retard from neutral."@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:integer;
  rdfs:label "lowStep";
  cims:multiplicity cims:M:1..1 .

cim:TapChanger.ltcFlag a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Specifies whether or not a TapChanger has load tap changing capabilities."@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:boolean;
  rdfs:label "ltcFlag";
  cims:multiplicity cims:M:1..1 .

cim:TapChanger.neutralStep a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """The neutral tap step position for this winding.
The attribute shall be equal to or greater than lowStep and equal or less than highStep.
It is the step position where the voltage is neutralU when the other terminals of the transformer are at the ratedU.  If there are other tap changers on the transformer those taps are kept constant at their neutralStep."""@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:integer;
  rdfs:label "neutralStep";
  cims:multiplicity cims:M:1..1 .

cim:TapChanger.neutralU a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment "Voltage at which the winding operates at the neutral tap setting. It is the voltage at the terminal of the PowerTransformerEnd associated with the tap changer when all tap changers on the transformer are at their neutralStep position.  Normally neutralU of the tap changer is the same as ratedU of the PowerTransformerEnd, but it can differ in special cases such as when the tapping mechanism is separate from the winding more common on lower voltage transformers. This attribute is not relevant for PhaseTapChangerAsymmetrical,"@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:float;
  rdfs:label "neutralU";
  cims:multiplicity cims:M:1..1;
  cim:unitMultiplier cim:UnitMultiplier.k;
  cim:unitSymbol cim:UnitSymbol.V;
  qudt:hasQuantityKind cim:Voltage;
  qudt:hasUnit unit:KiloV .

cim:TapChanger.normalStep a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """The tap step position used in \"normal\" network operation for this winding. For a \"Fixed\" tap changer indicates the current physical tap setting.
The attribute shall be equal to or greater than lowStep and equal to or less than highStep."""@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:integer;
  rdfs:label "normalStep";
  cims:multiplicity cims:M:1..1 .

cim:TapChanger.step a owl:FunctionalProperty, owl:DatatypeProperty;
  rdfs:comment """Tap changer position.
Starting step for a steady state solution. Non integer values are allowed to support continuous tap variables. The reasons for continuous value are to support study cases where no discrete tap changer has yet been designed, a solution where a narrow voltage band forces the tap step to oscillate or to accommodate for a continuous solution as input.
The attribute shall be equal to or greater than lowStep and equal to or less than highStep."""@en;
  rdfs:domain cim:TapChanger;
  rdfs:range xsd:float;
  rdfs:label "step";
  cims:multiplicity cims:M:1..1 .

cim:TapChangerControl a owl:Class;
  rdfs:comment "Describes behaviour specific to tap changers, e.g. how the voltage at the end of a line varies with the load level and compensation of the voltage drop by tap adjustment."@en;
  rdfs:subClassOf cim:RegulatingControl;
  rdfs:label "TapChangerControl" .

cim:TapChangerControl.TapChanger a owl:ObjectProperty, owl:InverseFunctionalProperty;
  rdfs:comment "The tap changers that participates in this regulating tap control scheme."@en;
  rdfs:domain cim:TapChangerControl;
  rdfs:range cim:TapChanger;
  rdfs:label "TapChanger";
  cims:multiplicity cims:M:1..n;
  owl:inverseOf cim:TapChanger.TapChangerControl .

cim:TapChangerInfo a owl:Class;
  rdfs:comment "Tap changer data."@en;
  rdfs:subClassOf cim:AssetInfo;
  rdfs:label "TapChangerInfo" .

