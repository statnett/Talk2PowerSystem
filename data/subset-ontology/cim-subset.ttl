cim:SwitchInfo.isUnganged a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "If true, the switch is not ganged (i.e., a switch phase may be operated separately from other phases)."@en;
  rdfs:domain cim:SwitchInfo;
  rdfs:label "isUnganged";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:Boolean .

cim:SwitchInfo.ratedCurrent a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Rated current."@en;
  rdfs:domain cim:SwitchInfo;
  rdfs:label "ratedCurrent";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:CurrentFlow .

cim:SwitchInfo.ratedVoltage a owl:ObjectProperty, owl:FunctionalProperty;
  rdfs:comment "Rated voltage."@en;
  rdfs:domain cim:SwitchInfo;
  rdfs:label "ratedVoltage";
  cims:multiplicity cims:M:0..1;
  cims:dataType cim:Voltage .

cim:SynchronousMachine a owl:Class;
