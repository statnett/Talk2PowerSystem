@prefix unit:         <http://qudt.org/vocab/unit/> .
@prefix qudt:         <http://qudt.org/schema/qudt/> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix sou:           <http://qudt.org/vocab/sou/> .
@prefix cim:          <https://cim.ucaiug.io/ns#> .
@prefix rdfs:         <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos:         <http://www.w3.org/2004/02/skos/core#> .
@prefix dct:          <http://purl.org/dc/terms/> .
@prefix xsd:          <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:          <http://www.w3.org/2002/07/owl#> .


<https://cim.ucaiug.io/qudt> a owl:Ontology ;
    rdfs:label "CIM QUDT units ";
    rdfs:comment "Units in CIM missing from QUDT";
    dct:created "2025-09-12"^^xsd:date;
    dct:creator "Graphwise AI";
    rdfs:seeAlso
    <https://github.com/statnett/Talk2PowerSystem_PM/issues/213>,
    <https://github.com/qudt/qudt-public-repo/issues/970>,
    <https://github.com/Sveino/Inst4CIM-KG/tree/develop/rdfs-improved#mapping-quantitykinds-and-units>
.

unit:CCY_NOK-PER-MegaW-HR
    a qudt:Unit ;
    qudt:applicableSystem sou:SI ;
    #  qudt:conversionMultiplier 0.0000002777777777777777777777777777777778 ;
    #  qudt:conversionMultiplierSN 2.777777777777777777777777777777778E-7 ;
    #  qudt:hasDimensionVector qkdv:A0E0L-2I0M-1H0T2D0 ;
    qudt:hasFactorUnit [
          qudt:exponent -1 ;
          qudt:hasUnit unit:HR ;
      ] ;
    qudt:hasFactorUnit [
          qudt:exponent -1 ;
          qudt:hasUnit unit:MegaW ;
      ] ;
    qudt:hasFactorUnit [
          qudt:exponent 1 ;
          qudt:hasUnit unit:CCY_NOK ;
      ] ;
    qudt:hasQuantityKind quantitykind:CostPerEnergy ;
    qudt:plainTextDescription "Unit for measuring the cost of electricity." ;
    qudt:symbol "NOK/(MW·h)" ;
    rdfs:isDefinedBy <https://cim.ucaiug.io/qudt> ;
    rdfs:label "Norwegian Krone per Megawatt Hour" ;
    rdfs:label "Norwegian Krone Megawatt Hour"@en ;
.
quantitykind:CostPerEnergy qudt:applicableUnit unit:CCY_NOK-PER-MegaW-HR .


unit:CCY_EUR-PER-MegaW-HR
    a qudt:Unit ;
    qudt:applicableSystem sou:SI ;
    #  qudt:conversionMultiplier 0.0000002777777777777777777777777777777778 ;
    #  qudt:conversionMultiplierSN 2.777777777777777777777777777777778E-7 ;
    #  qudt:hasDimensionVector qkdv:A0E0L-2I0M-1H0T2D0 ;
    qudt:hasFactorUnit [
        qudt:exponent -1 ;
        qudt:hasUnit unit:HR ;
        ] ;
    qudt:hasFactorUnit [
        qudt:exponent -1 ;
        qudt:hasUnit unit:MegaW ;
        ] ;
    qudt:hasFactorUnit [
        qudt:exponent 1 ;
        qudt:hasUnit unit:CCY_EUR ;
        ] ;
    qudt:hasQuantityKind quantitykind:CostPerEnergy ;
    qudt:plainTextDescription "Unit for measuring the cost of electricity." ;
    qudt:symbol "€/(MW·h)" ;
    rdfs:isDefinedBy <https://cim.ucaiug.io/qudt> ;
    rdfs:label "Euro per Megawatt Hour" ;
    rdfs:label "Euro per Megawatt Hour"@en ;
.
quantitykind:CostPerEnergy qudt:applicableUnit unit:CCY_EUR-PER-MegaW-HR .

## exchange rate


unit:CCY_DKK-PER-EUR
    a qudt:Unit ;
    qudt:hasQuantityKind quantitykind:CurrencyExchangeRate ;
    qudt:hasFactorUnit [
        qudt:hasUnit unit:CCY_DKK;
        qudt:exponent 1
        ];
    qudt:hasFactorUnit [
        qudt:hasUnit unit:CCY_EUR;
        qudt:exponent -1
        ];
    rdfs:label "Danish Krone per Euro" ;
    qudt:symbol "DKK/EUR" ;
    qudt:plainTextDescription "Exchange rate of Danish Krone to Euros" ;
    rdfs:isDefinedBy <https://cim.ucaiug.io/qudt> ;
.

unit:CCY_NOK-PER-EUR
    a qudt:Unit ;
    qudt:hasQuantityKind quantitykind:CurrencyExchangeRate ;
    qudt:hasFactorUnit [
        qudt:hasUnit unit:CCY_NOK;
        qudt:exponent 1
        ];
    qudt:hasFactorUnit [
        qudt:hasUnit unit:CCY_EUR;
        qudt:exponent -1
        ];
    rdfs:label "Norwegian Krone per Euro" ;
    qudt:symbol "NOK/EUR" ;
    qudt:plainTextDescription "Exchange rate of Norwegian Krone to Euros" ;
    rdfs:isDefinedBy <https://cim.ucaiug.io/qudt> ;
.

unit:CCY_SEK-PER-EUR
    a qudt:Unit ;
    qudt:hasQuantityKind quantitykind:CurrencyExchangeRate ;
    qudt:hasFactorUnit [
        qudt:hasUnit unit:CCY_SEK;
        qudt:exponent 1
        ];
    qudt:hasFactorUnit [
        qudt:hasUnit unit:CCY_EUR;
        qudt:exponent -1
        ];
    rdfs:label "Swedish Krona per Euro" ;
    qudt:symbol "SEK/EUR" ;
    qudt:plainTextDescription "Exchange rate of Swedish Krona to Euros" ;
    rdfs:isDefinedBy <https://cim.ucaiug.io/qudt> ;
.

quantitykind:CurrencyExchangeRate
    a qudt:QuantityKind ;
    rdfs:label "Currency Exchange Rate" ;
    qudt:applicableUnit unit:CCY_SEC-PER-EUR, unit:CCY_NOK-PER-EUR,unit:DKK_NOK-PER-EUR ;
    rdfs:isDefinedBy <https://cim.ucaiug.io/qudt>
.

